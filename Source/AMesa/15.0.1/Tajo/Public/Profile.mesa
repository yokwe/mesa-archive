CEDURE [name, prefix, suffix: String ¬ noChange];  SetUser: PROCEDURE [name, password: String ¬ noChange];  SetSwapCtrlAndCommand: PROCEDURE [BOOLEAN];    Qualification: TYPE = {registry, clearinghouse, none};    Qualify: PROCEDURE [token, newToken: String, qualification: Qualification];  END.