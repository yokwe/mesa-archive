AddrImpl, StubImpl »;  PACK  --NSStringConfig  NSNameImpl, NSNameExtrasImpl, XFormatSCSImpl, NSStringImpl,  --NSStringConvertConfig  NSStringConvertImpl, XC82ToInterimImpl, InterimToXC82Impl, StandardToXC82Impl,  XC82ToStandardImpl, InterimToStandardKanjiMap, StandardToInterimKanjiMap,  --XComSoft  XTimeImpl; PACK  NSSessionImpl,   -- NSFilingRemoteConfig  NSFileCourierClientImpl, NSFileCourierServerImpl, NSFileCourierUtilityAImpl,  NSFileCourierUtilityBImpl, NSFilingControlAImpl, NSFilingControlBImpl,  HandleManagerImpl, MacroOpsImpl, NSFileNameImpl, NSFileStubOnlyImpl; Othello: CONFIGURATION   IMPORTS    -- BasicHeads    BeepFace, DisplayFace, EthernetFace, HeadStartChain, KeyboardFace, MouseFace,    PilotDiskFace, --PilotDiskFaceExtra, --ProcessorFace, RealMemory,     RealMemoryExtras, FloppyDiskFace, TemporarySetGMT, TTYPortFace,    SpecialFloppyTapeFace,    --for SimpleTTY&TTYLearSiegler (unused) stuff    BitBlt, Inline, Format,    --for TTYLearSiegler    TTY,    OthelloDefs--[GetCannedScript, SuppressOutput, ThereIsAnError]--,    RuntimeInternal, RuntimePrograms, --no loader--     MStream, Exec, Time, Profile,  -- for AddressTranslationConfig    NSVolumeControl, NSVolumeInternal, ClientOpsInternal, XComSoftMessage,    XMessage, CHStubInternal, ProcessorFaceExtras2  EXPORTS -- for BasicHeads    DeviceCleanup, Process, ResidentHeap, SpecialRuntime, SpecialSpace,    SpecialSystem, System =  BEGIN  AdmConfig: CONFIG     IMPORTS      Heap, Process, RealMemory, SpecialRuntime, Stream, Space, TTY, TTYPortFace    EXPORTS TTY    CONTROL FreeAllReservedMemory, TTYPortChannel, TTYLearSiegler = {    FreeAllReservedMemory; TTYPortChannel; TTYLearSiegler};  -- This nested config is required to prevent early start traps  OthelloConfig: CONFIG -- not IMPORTed warnings OK here.    IMPORTS      Auth, AddressTranslation, BitBlt, Buffer, ByteBlt, CH,       Courier, DiskChannel,       ExtendedString, File, FloppyChannel, Format, Heap, Inline, KernelFile,      NSDataStream, NSFile, NSName, NSString, OthelloDefs, PacketExchange,      PhysicalVolume, PilotDiskFace, Process, ProcessorFace, ProcessorFaceExtras2,      ResidentHeap, Router, Runtime, Scavenger, Socket,       Space, SpecialBooting, SpecialRuntime, SpecialSpace, SpecialSystem,       SpecialVolume, Stream, System,       TemporaryBooting, TemporaryBootingExtras, TemporarySetGMT,       TTY, UserTerminal, UserTerminalExtras2, Version, VersionExtras, VM, Volume    EXPORTS      Format, PilotClient, Profile, String    CONTROL      SubOthello, OthelloDeviceImplD0DLion = {    SubOthello;    OthelloDeviceImplD0DLion};  AddressTranslationConfig;  PilotKernel;  Base;  CourierConfig;  --ExtendedStringImpl;  MemoryStreamImpl;  HeapImpl;  VersionImpl;  XNS;    OthelloConfig;      AdmConfig;    NSSessionImpl;  NSFilingRemoteConfig;  NSStringConfig;  NSStringConvertConfig;  XComSoft;  AuthStub;  CHStub;    END.....