ce, ResidentHeap, Runtime,      RuntimeInternal, Scavenger, Snapshot, Space, <<SpacePerf,>> SpecialFile,      SpecialMFile, SpecialRuntime, SpecialSpace, SpecialSystem, SpecialVolume,      Stream, System, TemporaryBooting, TemporarySetGMT, Time, Tool,      UserTerminal, VM, Volume, Zone, ProcessorFaceExtras2    EXPORTS      PilotClient    CONTROL      -- Main program and independent packages:      UtilityPilotClientImpl, PilotSpecialDuke, MiniMugImplDuke,      TransactionExerciser, InstallBootFiles,      -- Support software:      FormatImpl, TTYImpl, SimpleTTY, StorageImpl,      StringsImplA, StringsImplB, TimeImpl, RuntimePerf =    BEGIN    -- Main program and independent packages:    UtilityPilotClientImpl LINKS: CODE;    PilotSpecialDuke LINKS: CODE;    MiniMugImplDuke;   << Timer;>>    InstallBootFiles;    TransactionExerciser;    -- Support software:    FormatImpl;    TTYImpl;    SimpleTTY;    StorageImpl;    StringsImplA;    StringsImplB;    TimeImpl;    RuntimePerf;    FloppyImpl;    OthelloOpsImpl;    END;  --MiniMuggerConfigDuke--    BasicHeadsDuke;  PilotKernel;  HeapImpl;  MiniMuggerConfigDuke;      END.LOG26-Apr-89 15:41:48  MXK  Created File from MiniMuggerOnUtilityPilotDove.config