buggerOutloaded: SpaceUsage.MesaUsage = firstDebuggerUsage+5;  debuggerScratchSpace: SpaceUsage.MesaUsage = firstDebuggerUsage+6;  debuggerSymbolSegment: SpaceUsage.MesaUsage = firstDebuggerUsage+7;  debuggerTeledebugScratchSpace: SpaceUsage.MesaUsage = firstDebuggerUsage+8;  debuggerVMMap: SpaceUsage.MesaUsage = firstDebuggerUsage+9;  -- DF tools usages  firstDFToolsUsage: SpaceUsage.MesaUsage = firstDebuggerUsage+10;    dfBcdBuffer: SpaceUsage.MesaUsage = firstDFToolsUsage+0;  dfBootFileBuffer: SpaceUsage.MesaUsage = firstDFToolsUsage+1;  dfHugeZone: SpaceUsage.MesaUsage = firstDFToolsUsage+2;  dfParsedDFSequence: SpaceUsage.MesaUsage = firstDFToolsUsage+3;  dfPhase1BTree: SpaceUsage.MesaUsage = firstDFToolsUsage+4;  dfPhase3BTree: SpaceUsage.MesaUsage = firstDFToolsUsage+5;  dfStatsBcdBuffer: SpaceUsage.MesaUsage = firstDFToolsUsage+6;  -- File System usages  firstFileSystemUsage: SpaceUsage.MesaUsage = firstDFToolsUsage+7;    bTreeDebug: SpaceUsage.MesaUsage = firstFileSystemUsage+0;  mFileDirectory: SpaceUsage.MesaUsage = firstFileSystemUsage+1;  mFileLeaderPage: SpaceUsage.MesaUsage = firstFileSystemUsage+2;  mFileLocalStream: SpaceUsage.MesaUsage = firstFileSystemUsage+3;  mFileRemoteStream: SpaceUsage.MesaUsage = firstFileSystemUsage+4;  mFileScavenge: SpaceUsage.MesaUsage = firstFileSystemUsage+5;  rFileServerBuffer: SpaceUsage.MesaUsage = firstFileSystemUsage+6;  -- IncludeChecker usages  firstIncludeCheckerUsage: SpaceUsage.MesaUsage = firstFileSystemUsage+7;    includeCheckerInputBcdBuffer: SpaceUsage.MesaUsage = firstIncludeCheckerUsage+0;  -- InternalTools usages  firstInternalToolUsage: SpaceUsage.MesaUsage = firstIncludeCheckerUsage+1;    bcdToolBcdSegment: SpaceUsage.MesaUsage = firstInternalToolUsage+0;  cpDebugBcdSegment: SpaceUsage.MesaUsage = firstInternalToolUsage+1;  cpDebugMemCache: SpaceUsage.MesaUsage = firstInternalToolUsage+2;  -- Librarian usages  firstLibrarianUsage: SpaceUsage.MesaUsage = firstInternalToolUsage+3;    librarianMDSStorage: SpaceUsage.MesaUsage = firstLibrarianUsage+0;  -- Lister usages  firstListerUsage: SpaceUsage.MesaUsage = firstLibrarianUsage+1;    listerBcdBuffer: SpaceUsage.MesaUsage = firstListerUsage+0;  listerCodeBuffer: SpaceUsage.MesaUsage = firstListerUsage+1;  listerImplementationBcdBuffer: SpaceUsage.MesaUsage = firstListerUsage+2;  listerSymbolsBuffer: SpaceUsage.MesaUsage = firstListerUsage+3;  listerUnboundExportBcdBuffer: SpaceUsage.MesaUsage = firstListerUsage+4;  -- Mail usages  firstMailUsage: SpaceUsage.MesaUsage = firstListerUsage+5;    mailAnswer: SpaceUsage.MesaUsage = firstMailUsage+0;  mailFileExpunge: SpaceUsage.MesaUsage = firstMailUsage+1;  mailRetrieve: SpaceUsage.MesaUsage = firstMailUsage+2;  -- MakeBoot usages  firstMakeBootUsage: SpaceUsage.MesaUsage = firstMailUsage+3;    makeBootBcdBuffer: SpaceUsage.MesaUsage = firstMakeBootUsage+0;  makeBootBcdVersionBuffer: SpaceUsage.MesaUsage = firstMakeBootUsage+1;  makeBootCopyCodeBuffer: SpaceUsage.MesaUsage = firstMakeBootUsage+2;  makeBootCopyGermCodeBuffer: SpaceUsage.MesaUsage = firstMakeBootUsage+3;  makeBootGetPages: SpaceUsage.MesaUsage = firstMakeBootUsage+4;  -- Packager usages  firstPackagerUsage: SpaceUsage.MesaUsage = firstMakeBootUsage+5;    packagerBCD: SpaceUsage.MesaUsage = firstPackagerUsage+0;  packagerBuffer: SpaceUsage.MesaUsage = firstPackagerUsage+1;  packagerSymbolTable: SpaceUsage.MesaUsage = firstPackagerUsage+2;  packagerTextBuffer: SpaceUsage.MesaUsage = firstPackagerUsage+3;  -- PGS usages  firstPGSUsage: SpaceUsage.MesaUsage = firstPackagerUsage+4;    pgsBCD: SpaceUsage.MesaUsage = firstPGSUsage+0;  pgsBuffer: SpaceUsage.MesaUsage = firstPGSUsage+1;  pgsSymbolTable: SpaceUsage.MesaUsage = firstPGSUsage+2;  pgsTextBuffer: SpaceUsage.MesaUsage = firstPGSUsage+3;  -- Print usages  firstPrintUsage: SpaceUsage.MesaUsage = firstPGSUsage+4;    printData: SpaceUsage.MesaUsage = firstPrintUsage+0;