--FlushServiceCache.mesa--RSF	13-Aug-85 18:31:24DIRECTORY  NSExec,  NSFilingGlobal,  NSString;FixServiceCache: PROGRAM  IMPORTS NSExec, NSFilingGlobal, NSString =    BEGIN   S: PROC [s: LONG STRING] RETURNS [NSString.String] = INLINE {    RETURN[NSString.StringFromMesaString[s]]};      FlushCache: NSExec.ExecProc = {    NSFilingGlobal.FlushServiceCache[]};    Init: PROCEDURE = {    NSExec.AddExecCommand[     command:[name: S["FlushServiceCache"L], proc: FlushCache]]};      Init[];  END. -- of FixServiceCache