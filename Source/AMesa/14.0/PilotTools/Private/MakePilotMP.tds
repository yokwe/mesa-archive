-- MakePilotMP.tds   30-Mar-82 14:02:47-- Script to run Adobe tools necessary to generate input to MakePilotMPActivateTool["AdobeQuery"]WindowOnTop[AdobeQuery]TOOL _ "AdobeQuery"SUBWINDOW _ "formSW"InvokeMCR[AdobeQuery.formSW, "Adobe systems", "Dandelion MPCodes"]MPCode _ ">= 900 AND < 1000"cmdsw.QueryActivateTool["Adobe Report"]WindowOnTop[AdobeReport]TOOL _ "AdobeReport"SUBWINDOW _ "cmdsw"InvokeMCR[AdobeReport.cmdsw, "Adobe systems", "Dandelion MPCodes"]Output' format _ "template"Overwrite _ TRUETemplate' file _ "MakePilotMP.template"Output' file _ "PilotMP.unsorted"cmdsw.Set' FieldssortSW.MPCode _ TRUEcmdsw.ReportActivateTool["Adobe Sort"]WindowOnTop[AdobeSort]TOOL _ "AdobeSort"SUBWINDOW _ "formSW"Input' File _ AdobeReport.cmdsw.Output' fileOutput' File _ "PilotMP.report"Key' specs _ "MPCode"Sort\