tDiskFace, Process, ProcessorFace, ResidentHeap,       Runtime, RuntimeInternal, Scavenger, Snapshot, Space, SpacePerf,       SpecialFile, SpecialMFile, SpecialRuntime, SpecialSpace, SpecialSystem,       SpecialVolume, Stream, System, TemporaryBooting, TemporarySetGMT, Time,       Tool, UserTerminal, VM, Volume, Zone    EXPORTS      PilotClient    CONTROL      -- Main program and independent packages:      UtilityPilotClientImpl, PilotTests, MiniMugImpl,      Timer, TransactionExerciser, InstallBootFiles,      -- Support software:      FormatImpl, TTYImpl, SimpleTTY, StorageImpl,      StringsImplA, StringsImplB, TimeImpl, RuntimePerf =    BEGIN    -- Main program and independent packages:    UtilityPilotClientImpl LINKS: CODE;    PilotTests LINKS: CODE;    MiniMugImpl;    Timer;    InstallBootFiles;    TransactionExerciser;    -- Support software:    FormatImpl;    TTYImpl;    SimpleTTY;    StorageImpl;    StringsImplA;    StringsImplB;    TimeImpl;    RuntimePerf;    FloppyImpl;    OthelloOpsImpl;    END;  --PilotTestsConfig--    BasicHeadsDove;  PilotKernel;  HeapImpl;  PilotTestsConfig;      END.LOG29-Dec-87 14:40:09	CAJ	Created file.