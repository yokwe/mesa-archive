, largeVector(8), packedPixelVector(9), compressedPixelVector(10), insertFile(11)};  MasterEncodingType: TYPE = MACHINE DEPENDENT {    shortNumber0(0), shortNumber1(1), shortNumber2(2), shortNumber3(3),    shortOp(4), longOp(5), shortSequence(6), longSequence(7) };    MasterEncoding: TYPE = MACHINE DEPENDENT RECORD [ -- the guys in the encoded master --    body(0:0..15): SELECT type(0:0..2): MasterEncodingType FROM      shortNumber0 => [lowBits(0:3..15): [0..8192)],      shortNumber1 => [lowBits(0:3..15): [0..8192)],      shortNumber2 => [lowBits(0:3..15): [0..8192)],      shortNumber3 => [lowBits(0:3..15): [0..8192)],      shortOp => [op(0:3..7): SmallOperatorEnumeration, pad(0:8..15): [0..255]],      longOp => [op(0:3..15): OperatorEnumeration],      shortSequence => [type(0:3..7): SequenceType, length(0:8..15): [0..256)],      longSequence => [type(0:3..7): SequenceType, lengthHi(0:8..15): [0..256) -- continued --],      ENDCASE];    ImagerVariable: TYPE = MACHINE DEPENDENT {    DCScpx(0), DCScpy(1),    correctMX(2), correctMY(3),    T(4),    priorityImportant(5),    mediumXSize(6), mediumYSize(7),    fieldXMin(8), fieldYMin(9),    fieldXMax(10), fieldYMax(11),    showVec(12),    color(13),    noImage(14),    strokeWidth(15),    strokeEnd(16),    underlineStart(17),    amplifySpace(18),    correctPass(19),    correctShrink(20),    correctTX(21), correctTY(22)};  END.