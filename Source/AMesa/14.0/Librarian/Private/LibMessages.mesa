se i didn't want to renumber all the stupid things      keyLibjectName,      keyCheckingInLibject,      keyNotCheckedOut,      keyNoSuchDatabase,      keyUnknownLibrarianError,      keyNotFound,      keyNotProductFactored,      keyLibrarianService,      keyIllegalDatabaseName,      keyOutOfVirtualMemory,      keyLS            };      totalKeys: CARDINAL = ORD[MessageKey.LAST] - ORD[MessageKey.FIRST] + 1;  GetMessages: PROCEDURE RETURNS [h: XMessage.Handle];  -- Utility procs  M: PROCEDURE [key: MessageKey] RETURNS [string: NSString.String];  PutMsg: PROCEDURE [    e: NSExec.Handle, template: NSString.String,    s1, s2, s3, s4: NSString.String ¬ NSString.nullString];  ExpandMsg: PROCEDURE [    z: UNCOUNTED ZONE, template: NSString.String,    s1, s2, s3, s4: NSString.String ¬ NSString.nullString]    RETURNS [result: NSString.String];  NoteEvent: PROCEDURE [s: NSString.String];  END.  -- LibMessages  LOG [Time - Person - Action] 5-Aug-86 16:46:23 - bjd - Created file