-- Overlap.mesa,  4-Jul-83 13:44:42Overlap: DEFINITIONS =  BEGIN    zone: UNCOUNTED ZONE;    Interval: TYPE = LONG POINTER TO IntervalRecord;  IntervalRecord: TYPE = RECORD [start, stop: LONG INTEGER, user: LONG POINTER];    IntervalSet: TYPE = LONG POINTER TO IntervalSetSequence;  IntervalSetSequence: TYPE = RECORD [set: SEQUENCE size: CARDINAL OF Interval];    ConstructCorrectInterval: PROCEDURE [set: IntervalSet, maxErr: CARDINAL]    RETURNS [correct: IntervalRecord];      IncorrectSubset: PROCEDURE [set: IntervalSet, maxErr: CARDINAL]    RETURNS [correct, incorrect: IntervalSet];    BadInterval: SIGNAL [who: CARDINAL]; -- RESUME will correct interval  BadMaxErr: ERROR [smallest: CARDINAL];  BadSet: ERROR;    END...