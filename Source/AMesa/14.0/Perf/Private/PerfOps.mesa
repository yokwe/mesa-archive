ram, DeleteHistogram, PrintHistogram: PROC;  InitSession: PROC;  CmdIndex: TYPE = {    title, mon, condition, collect, addleg, from, to, delete, leg, printtables,    printnodes, printlegs, zero, init};  CmdItems: TYPE = LONG DESCRIPTOR FOR ARRAY CmdIndex OF FormSW.ItemHandle;  ModeIndex: TYPE = {mode, add, track, setProcess, process};    ModeItems: TYPE = LONG DESCRIPTOR FOR ARRAY ModeIndex OF FormSW.ItemHandle;    HistCIndex: TYPE = {    histcommands, addhist, delhist, printhist, histType, histClass, histNode,    histLeg, buckets, scale, base};    HistItems: TYPE = LONG DESCRIPTOR FOR ARRAY HistCIndex OF FormSW.ItemHandle;  ParamNotify: FormSW.NotifyProcType;  ParamEnumNotify: FormSW.EnumeratedNotifyProcType;  GetFromNode: PROC RETURNS [NodeIndex];  GetToNode: PROC RETURNS [NodeIndex];  GetDeleteLeg: PROC RETURNS [LegIndex];  GetHistNode: PROC RETURNS [NodeIndex];  GetHistLeg: PROC RETURNS [LegIndex];  GetBuckets: PROC RETURNS [CARDINAL];  GetScale: PROC RETURNS [CARDINAL];  GetBase: PROC RETURNS [LONG CARDINAL];  monitorOn: BOOLEAN;  addMode: LegAddClass;  trackMode: LegTrackClass;  process: LONG STRING;  histType: HistType;  histClass: HistClass;  END..