 u5(5), u6(6), u7(7),    u8(8), u9(9), u10(10), u11(11), u12(12), u13(13), u14(14), u15(15)};  SpecialType: TYPE= MACHINE DEPENDENT{    TPC0(0), TPC1(1), TPC2(2), TPC3(3), TPC4(4), TPC5(5), TPC6(6), TPC7(7),    lastBlock(8), loadIOP(9), loadU(10), ignore(11), startIOP(12),    loadCPMemory(13)};  BootDevice: TYPE= MACHINE DEPENDENT{    unconditional(0), SA4000(1), SA1000(2), ether(3), floppy(4), trident(5),    placeHolder(15)};  LastBlockFlags: TYPE=    MACHINE DEPENDENT RECORD[inhibitStartCP: BOOLEAN, unused: [0..77777B]];  uAddress: TYPE= [0..7777B];  --Bits in real storage location 0 used by phase 0 microcode to indicate--  --presence of devices.  Included here only for reference.--  SA4000Bit: CARDINAL= 1;  SA1000Bit: CARDINAL= 2;  EtherBit: CARDINAL= 4;  TridentBit: CARDINAL= 8;  END.