--Lex.pack  Lex: SEGMENT =    BEGIN    LexicalStringManagement: CODE PACK =      BEGIN      Lexicon EXCEPT CollectAndDispatchCommands, [MAIN, CATCH CODE];      LexiconClient[ENTRY VECTOR];      END;    CollectAndDispatchCommands: CODE PACK =      BEGIN      Lexicon[PrintLexicon];      LexiconClient EXCEPT [ENTRY VECTOR, CATCH CODE];      END;    InitAndSeldomUsed: CODE PACK =      BEGIN      LexiconClient[CATCH CODE];      Lexicon[MAIN, CATCH CODE];      END;    END;        --Frame packs        UtilityFrames: FRAME PACK = {Lexicon};        DriverFrame: FRAME PACK ={LexiconClient}.