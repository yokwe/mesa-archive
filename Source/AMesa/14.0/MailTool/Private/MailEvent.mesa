-- File: MailEvent.mesa - last edit:-- AlHall.PA           24-Aug-84 21:42:50-- Provides for type-safe access of MailEvent.Data, since--   normal type-checking is lost when eventData is reduced--   to a LONG POINTER via Supervisor.NotifyDirectSubsystems.MailEvent: DEFINITIONS =BEGIN-- TYPEs:Data: TYPE = LONG POINTER TO DataObject;DataObject: TYPE = RECORD [  stamp: Stamp ¬ dataStamp,  version: CARDINAL ¬ dataVersion,  newMailPending: BOOLEAN ¬ FALSE,  transport: Transport ¬ ns,  userData: UserData ¬ NIL];Stamp: TYPE = LONG CARDINAL;Transport: TYPE = {gv, ns};UserData: TYPE = LONG POINTER;-- Constants:dataStamp: Stamp = 4945842;dataVersion: CARDINAL = 1;END.