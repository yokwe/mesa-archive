CHARACTER], s: LONG STRING];  -- don't interpret special chars  FWF0: PROC [proc: PROC [CHARACTER], s: LONG STRING];  FWF1: PROC [proc: PROC [CHARACTER], s: LONG STRING, a: LONG POINTER];  FWF2: PROC [proc: PROC [CHARACTER], s: LONG STRING, a, b: LONG POINTER];  FWF3: PROC [proc: PROC [CHARACTER], s: LONG STRING, a, b, c: LONG POINTER];  FWF4: PROC [proc: PROC [CHARACTER], s: LONG STRING, a, b, c, d: LONG POINTER];  FWFN: PROC [    proc: PROC [CHARACTER], s: LONG STRING,    array: DESCRIPTOR FOR ARRAY OF LONG POINTER];  FWF: PROC [    proc: PROC [CHARACTER], s: LONG STRING, a, b, c, d: LONG POINTER ¬ NIL];  FWFC: PROC [proc: PROC [CHARACTER], ch: CHARACTER];  FWFCR: PROC [proc: PROC [CHARACTER]];  -- WriteFormatted to a Format.StringProc, you supply the procedure to call  PW0: PROC [proc: Format.StringProc, s: LONG STRING];  -- don't interpret special chars  PWF0: PROC [proc: Format.StringProc, s: LONG STRING];  PWF1: PROC [proc: Format.StringProc, s: LONG STRING, a: LONG POINTER];  PWF2: PROC [proc: Format.StringProc, s: LONG STRING, a, b: LONG POINTER];  PWF3: PROC [proc: Format.StringProc, s: LONG STRING, a, b, c: LONG POINTER];  PWF4: PROC [proc: Format.StringProc, s: LONG STRING, a, b, c, d: LONG POINTER];  PWFN: PROC [    proc: Format.StringProc, s: LONG STRING,    array: DESCRIPTOR FOR ARRAY OF LONG POINTER];  PWF: PROC [    proc: Format.StringProc, s: LONG STRING, a, b, c, d: LONG POINTER ¬ NIL];  PWFC: PROC [proc: Format.StringProc, ch: CHARACTER];  PWFCR: PROC [proc: Format.StringProc];  }.