    tellFileSystemSwappingOut: Supervisor.Event = [EventTypes.firstDebugEvent+8];  tellFileSystemSwappingIn: Supervisor.Event = [EventTypes.firstDebugEvent+9];  -- these may be vetoed: used with Event.aboutToSwap  aboutToBoot: Supervisor.Event = [firstVetoEvent];  aboutToAbortSession: Supervisor.Event = [firstVetoEvent+1];  aboutToResume: Supervisor.Event = [firstVetoEvent+2];  aboutToBootPhysicalVolume: Supervisor.Event = [firstVetoEvent+3];  -- FileSystem: used with Event.fileSystem  aboutToChangeSearchPath: Supervisor.Event = [firstFileSystem];  newSearchPath: Supervisor.Event = [firstFileSystem + 1];  abortedSearchPathChange: Supervisor.Event = [firstFileSystem + 2];  directoryCreated: Supervisor.Event = [firstFileSystem + 3];  directoryDeleted: Supervisor.Event = [firstFileSystem + 4];  volumeOpened: Supervisor.Event = [firstFileSystem + 5];  volumeClosed: Supervisor.Event = [firstFileSystem + 6];  aboutToOpenVolume: Supervisor.Event = [EventTypes.firstFileSystem+7];  aboutToCloseVolume: Supervisor.Event = [EventTypes.firstFileSystem+8];    -- basic Tajo defaults: used with Event.tajoDefaults  debugging: Supervisor.Event = [firstDefaultEvent+1];  librarian: Supervisor.Event = [firstDefaultEvent+2];  domain: Supervisor.Event = [firstDefaultEvent+3];  organization: Supervisor.Event = [firstDefaultEvent+4];  registry: Supervisor.Event = [firstDefaultEvent+5];  fileServerProtocol: Supervisor.Event = [EventTypes.firstDefaultEvent+6];  systemFont: Supervisor.Event = [EventTypes.firstDefaultEvent+7];  -- Credentials: used with Event.primaryCredentials and Event.secondaryCredentials  primaryCredentials: Supervisor.Event = [firstCredentialEvent];     -- FileWindow: used with Event.fileWindow  createWindow: Supervisor.Event = [firstFileWindowEvent];  destroy: Supervisor.Event = [firstFileWindowEvent+1];  edit: Supervisor.Event = [firstFileWindowEvent+2];  load: Supervisor.Event = [firstFileWindowEvent+3];  reset: Supervisor.Event = [firstFileWindowEvent+4];  store: Supervisor.Event = [firstFileWindowEvent+5];    -- ToolWindow: used with Event.toolWindow  createTool: Supervisor.Event = [firstToolWindowEvent];  activate: Supervisor.Event = [firstToolWindowEvent+1];  deactivate: Supervisor.Event = [firstToolWindowEvent+2]; -- this event can be vetoed -- used with Event.displayState  displayOff: Supervisor.Event = [firstDisplayEvent];  displayOn: Supervisor.Event = [firstDisplayEvent+1];    -- used with Event.powerOff  powerOff: Supervisor.Event = [firstOtherEvent];    -- Spares  spare1: Supervisor.Event = [firstSpare];   spare2: Supervisor.Event = [firstSpare+1];   spare3: Supervisor.Event = [firstSpare+2];   spare4: Supervisor.Event = [firstSpare+3];   spare5: Supervisor.Event = [firstSpare+4];     END.  