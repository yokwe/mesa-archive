   UserInputsA[CATCH CODE]};      UIMain: CODE PACK = {    MAIN OF TableSetup, TableMaker, UserInputsB;    TIPMatchImpl[MAIN];    TIPImpl EXCEPT TableSetup, [MAIN, CATCH CODE];    UserInputsA EXCEPT      StimLevResident, EVsAndOthers, MatcherMain, [CATCH CODE]};      END;