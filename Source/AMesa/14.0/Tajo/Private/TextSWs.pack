-- TextSWs.pack-- Last Edit:--  BGY     	 9-Feb-83 14:02:19TextSWs: SEGMENT =   BEGIN    TextSWEV: CODE PACK = {    FileSWsA    [ENTRY VECTOR];    MsgSWsA     [ENTRY VECTOR];    ScratchSWsA [ENTRY VECTOR];    StringSWsA  [ENTRY VECTOR];    TEditImpl   [ENTRY VECTOR];    TextSWsA    [ENTRY VECTOR];    TextSWsB    [ENTRY VECTOR];    TextSWsC    [ENTRY VECTOR];    TextSWsD    [ENTRY VECTOR];    TextSWsE    [ENTRY VECTOR];    TTYSWsA     [ENTRY VECTOR];    TTYSWsB     [ENTRY VECTOR];    };          TextSWCreate: CODE PACK = {    FileSWsA [Create, IsIt];    MsgSWsA [Create, IsIt];    ScratchSWsA [Create, IsIt];    StringSWsA [Create, IsIt];    TextSWsA [Create, IsIt];    TTYSWsA [Create, CreateInternal, CreateTTYInstance, CreateStream, IsIt];    TTYSWsB [CreateTTYInstance];    };      TextSWDestroy: CODE PACK = {    FileSWsA [Destroy, DestroyContext];    MsgSWsA [Destroy];    ScratchSWsA [Destroy, DestroyContext];    StringSWsA [Destroy, DestroyContext];    TextSWsA [Destroy, DestroyContext];    TTYSWsA [Destroy, DestroyTTYContext];    };      TextSWTypeIn: CODE PACK = {    TextSWsB[ModifyInsertionState];    TextSWsD [DequeueOutputRequest, EnqueueOutputRequest,       InsertBlock, InsertBlockInternal,       InternalForceOutput, InternalValidate, LocalFind, OutputBlock,       OutputString, OutputProcess, RealActOnCaret, TypeIn];    };      TextSWNotify: CODE PACK = {    TEditImpl EXCEPT TextSWEV, TEditCold, TextSWMain, TextSWMCR, TextSWCatch;    TextSWsE [InterpAtom, InterpretAtom, InterpInput];    };  TextSWSelect: CODE PACK = {    TextSWsA [SetSecondaryInternal, NewSecondaryObject];    TextSWsB[FinishExtend, FinishSelection, GetEndpointsForExtend,       InsertionAtSelection, InsertionToSelection, MarkSelectionInternal,       NormalizedPosition, StartExtend, StartSelection, TakeInputFocus,       TrackExtend, TrackExtendInternal, TrackSelection,       TrackSelectionInternal, UpdateSelectionInternal,       WithinClickTimeout];    TextSWsC[PToUse]    };      TextSWMCR: CODE PACK = {    TEditImpl [SearchForwards, SearchBackwards,        JFirst, JInsert, JLast, JSelect];    TextSWsB[SetSelection];    TextSWsC[FindMCR, SearchForSelection, JumpTopMCR, JumpEndMCR,       JumpInsertionMCR, JumpSelectionMCR, PositionMCR, WrapMCR];    };      OtherCold: CODE PACK = {    FileSWsA EXCEPT TextSWEV, TextSWCreate, TextSWDestroy,       TextSWMain, TextSWCatch;    MsgSWsA EXCEPT TextSWEV, TextSWCreate, TextSWDestroy,       TextSWMain, TextSWCatch;    ScratchSWsA EXCEPT TextSWEV, TextSWCreate,       TextSWDestroy, TextSWMain, TextSWCatch;    StringSWsA EXCEPT TextSWEV, TextSWCreate, TextSWDestroy,       TextSWMain, TextSWCatch;    };  TextSWsACold: CODE PACK = {    TextSWsA EXCEPT TextSWCatch, TextSWCreate, TextSWDestroy,       TextSWEV, TextSWMain, TextSWSelect;    };  TextSWsBCold: CODE PACK = {    TextSWsB EXCEPT TextSWCatch, TextSWEV, TextSWMain, TextSWMCR, TextSWSelect,       TextSWTypeIn;    };  TextSWsCCold: CODE PACK = {    TextSWsC EXCEPT TextSWCatch, TextSWEV, TextSWMain, TextSWMCR, TextSWSelect;    };  TextSWsDCold: CODE PACK = {    TextSWsD EXCEPT TextSWCatch, TextSWEV, TextSWMain, TextSWTypeIn;    };  TTYSWCold: CODE PACK = {    TTYSWsA EXCEPT TextSWCatch, TextSWCreate, TextSWDestroy, TextSWEV, TextSWMain;    };  TEditCold: CODE PACK = {    TEditImpl [Again, Blink, BracketSelection, Define, Expand,       FindField, FindNextField, GrayBox, InsertAngleBrackets,       InsertCurlyBrackets, InsertDoubleQuotes, InsertFieldBrackets,       InsertParens, InsertSingleQuotes, InsertSquareBrackets,       MakeLowerCase, MakeUpperCase,       ReplaceNextField, SelectConvertSwap, Splat, StrikeOut, Underline, Undo];    };  TextSWMain: CODE PACK = {    FileSWsA    [MAIN, Init];    MsgSWsA     [MAIN];    ScratchSWsA [MAIN];    StringSWsA  [MAIN];    TEditImpl   [MAIN, RegisterCommandAtoms, Register, UnRegister, IsRegistered];    TextSWsA    [MAIN];    TextSWsB    [MAIN];    TextSWsC    [MAIN];    TextSWsD    [MAIN, Init];    TextSWsE    [MAIN];    TTYSWsA     [MAIN];    TTYSWsB     [MAIN];    };  TextSWCatch: CODE PACK = {    FileSWsA    [CATCH CODE];    MsgSWsA     [CATCH CODE];    ScratchSWsA [CATCH CODE];    StringSWsA  [CATCH CODE];    TEditImpl   [CATCH CODE];    TextSWsA    [CATCH CODE];    TextSWsB    [CATCH CODE];    TextSWsC    [CATCH CODE];    TextSWsD    [CATCH CODE];    TextSWsE    [CATCH CODE];    TTYSWsA     [CATCH CODE];    TTYSWsB     [CATCH CODE];    };      END;  TextSWsFrames: FRAME PACK = { FileSWsA; MsgSWsA; ScratchSWsA; StringSWsA; TEditImpl; TextSWsA; TextSWsB; TextSWsC; TextSWsD; TextSWsE; TTYSWsA; TTYSWsB;   };   