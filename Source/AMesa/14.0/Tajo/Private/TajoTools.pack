XCEPT TajoToolEV, TajoToolCold;    };  ToolCreate: CODE PACK = {    ToolsA [Create, UserCmValues, GetState];    ToolsB [MakeDefaultSWs, MakeClientSW, MakeFileSW, AddThisSW,       MakeFormSW, MakeMsgSW, MakeStringSW, MakeTextSW, MakeTTYSW, UnusedLogName];    };  ToolDestroy: CODE PACK = {    ToolsA [Destroy, DestroyContext];    ToolsB [RemoveMenus, DeleteThisSW];    };--  ToolHot: CODE PACK = {--   ToolsA [TIPMe, PutSplitsDown, StartMoveSplit, Track, AdjustProc,  --      AdjustSubwindow, InvertBdryLine, SetNewBoxes, --      DetermineMovableBoundary];--    };  ToolsA: CODE PACK = {    ToolsA EXCEPT TajoToolEV, ToolCreate, ToolDestroy, TajoToolCold;    };  ToolsB: CODE PACK = {    ToolsB EXCEPT TajoToolEV, ToolCreate, ToolDestroy, TajoToolCold;    };      TajoToolCold: CODE PACK = {    EditMatch [MAIN, CATCH CODE];    EditUtilImplA [MAIN, CATCH CODE];    EditUtilImplB [MAIN, CATCH CODE];    EditTool [MAIN, CATCH CODE];    FileWindows [MAIN, CATCH CODE];    MenuSymbiote [MAIN, CATCH CODE];    SymbioteImpl [MAIN, CATCH CODE];    ToolsA [MAIN, CATCH CODE, InitMonitorData];    ToolsB [MAIN, CATCH CODE];    };      };  TajoToolFrames: FRAME PACK = {  EditTool; EditMatch; EditUtilImplA; EditUtilImplB; FileWindows;   MenuSymbiote; SymbioteImpl; ToolsA; ToolsB};    