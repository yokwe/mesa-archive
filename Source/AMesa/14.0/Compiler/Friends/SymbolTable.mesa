urgeProc: PROC [data: LONG POINTER, mdi: Symbols.MDIndex] ¬ NIL,    purgeData: LONG POINTER ¬ NIL,    link: Handle ¬ NIL,    zone: UNCOUNTED ZONE ¬ NIL,    fileTable: LONG POINTER TO FileTable ¬ NIL,    lastFile: INTEGER ¬ -1,    currentBody: Symbols.BTIndex ¬ Symbols.BTNull,    iHandle: Handle ¬ NIL,    memoCache: LONG POINTER TO CopierSearchCache ¬ NIL,    useMemo: BOOLEAN ¬ TRUE,    inDebugger: BOOLEAN ¬ FALSE,    typeCache: LONG POINTER TO TypeCache ¬ NIL];    CopierSearchCache: TYPE;  TypeCache: TYPE;    FileTable: TYPE;  SymSegHandle: TYPE [2];  NullHandle: SymSegHandle = LOOPHOLE[LONG[NIL]];      Missing: ERROR [handle: SymSegHandle];  Acquire: PROC [handle: SymSegHandle] RETURNS [Handle];  Release: PROC [h: Handle];  CacheSize: PROC RETURNS [pages: CARDINAL];  SetCacheSize: PROC [pages: CARDINAL];  SuspendCache, RestartCache: PROC;  IllegalBase: ERROR [h: Handle];  END.