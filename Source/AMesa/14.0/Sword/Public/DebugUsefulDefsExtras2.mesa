-- DebugUsefulDefsExtras2.mesaDIRECTORYEnvironment USING [PageNumber, PageCount],PageMap USING [Flags, RealPageNumber],Space USING [Access];DebugUsefulDefsExtras2: DEFINITIONS = {  SwapState: TYPE = {in, out, unmapped};    GetPageState: PROC[vmPage: Environment.PageNumber]     RETURNS[swapState: SwapState, access: Space.Access, flags: PageMap.Flags, real: PageMap.RealPageNumber];      GetVMSize: PROC[] RETURNS [page: Environment.PageCount];  }..