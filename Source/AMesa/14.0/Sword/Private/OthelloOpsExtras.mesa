-- OthelloOpsExtras.mesaDIRECTORYFile, Space, OthelloOps, StartList, Volume;OthelloOpsExtras: DEFINITIONS = {  StartListHeadPtr: TYPE = LONG POINTER TO StartList.Header;  SetGetSwitchesSuccess: TYPE = OthelloOps.SetGetSwitchesSuccess;  BootFileType: TYPE = OthelloOps.BootFileType;    NoStartListHeader: ERROR [code: SetGetSwitchesSuccess];  MapStartListHeader: PROC [    file: File.File, firstPage: File.PageNumber, access: Space.Access ¬ readWrite]    RETURNS [outcome: SetGetSwitchesSuccess, pStartListHeader: StartListHeadPtr];    GetVolumeBootFile: PROC[lvID: Volume.ID, type: BootFileType]    RETURNS [file: File.File, firstPage: File.PageNumber];  }..