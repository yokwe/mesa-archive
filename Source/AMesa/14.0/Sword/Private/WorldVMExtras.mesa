-- WorldVMExtras.mesaDIRECTORYEnvironment,PageMap,Space,TTY,WorldVM,WorldVMOps;WorldVMExtras: DEFINITIONS = {  Process: TYPE = WorldVMOps.Process;  Address: TYPE = WorldVMOps.Address;  NullProcess: Process = WorldVMOps.NullProcess;  ClientWorld: PROC[where, switches: LONG STRING, clientProcs: ClientProcs, tty: TTY.Handle ¬ TTY.nullHandle] RETURNS[WorldVM.World];    ClientProcs: TYPE = LONG POINTER TO ClientProcsObject;  ClientProcsObject: TYPE = RECORD[    Read: PROC[addr: Address, process: Process ¬ NullProcess] RETURNS[UNSPECIFIED],    Write: PROC[addr: Address, value: UNSPECIFIED, process: Process ¬ NullProcess],    CopyRead: PROC[from: Address, nwords: CARDINAL, to: LONG POINTER, process: Process ¬ NullProcess],    CopyWrite: PROC[from: LONG POINTER, nwords: CARDINAL, to: Address, process: Process ¬ NullProcess],    StopClient: PROC[]];    SetBreakNumber: PROC[world: WorldVM.World, number: CARDINAL];  GetBreakNumber: PROC[world: WorldVM.World] RETURNS[CARDINAL];    SwapState: TYPE = {in, out, unmapped};    GetPageState: PROC[world: WorldVM.World, addr: Address, process: Process ¬ NullProcess]     RETURNS[swapState: SwapState, access: Space.Access, flags: PageMap.Flags, real: PageMap.RealPageNumber];    WorldProc: TYPE = PROC[w: WorldVM.World] RETURNS[stop: BOOLEAN];  EnumerateWorlds: PROC[p: WorldProc] RETURNS[w: WorldVM.World];    }..