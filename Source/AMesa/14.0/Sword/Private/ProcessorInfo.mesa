-- ProcessorInfo.mesa created by DEG on 11-Jan-86 23:29:29-- last edited by DEG:	16-Apr-86 23:49:36ProcessorInfo: DEFINITIONS =  BEGIN    DisplayType: TYPE = {tty, largeFormatMonochrome, largeFormatColor};    FloatingPointStyle: TYPE = {software, microcode};  MachineType: TYPE = MACHINE DEPENDENT {    altoI(0), altoII(1), altoIIXM(2), dolphin(3), dorado(4), dandelion(5),    dicentra(6), daybreak(7), daisy(8), kiku(9), tridentDandelion(10),     unknown(LAST[CARDINAL])};      << Before any of the READONLY variables can be used at least one call must     be made on either GetMachineAttributes or GetMachineType. >>       cedarSupport: READONLY BOOLEAN;  displayType: READONLY DisplayType;  floatingPoint: READONLY FloatingPointStyle;  machineType: READONLY MachineType;      GetMachineAttributes: PROCEDURE    RETURNS[    machineType: MachineType, displayType: DisplayType,    floatingPoint: FloatingPointStyle, cedarSupport: BOOLEAN];    GetMachineType: PROCEDURE RETURNS[MachineType];    END...