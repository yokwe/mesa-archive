-- TestVectorIO.mesa--  1-Oct-84  9:27:12-- last edit by jAC   CxC     1-Oct-84  9:27:22DIRECTORY  Environment USING [Byte],  I186 USING [Address,Register],  OMF USING [DataSequence];TestVectorIO: DEFINITIONS =  BEGIN    Error: ERROR [reason: LONG STRING];    Boot: PROCEDURE;  Continue: PROCEDURE;  Load: PROCEDURE [log: LONG POINTER, filename: LONG STRING];    ReadMemoryByte: PROCEDURE [address: I186.Address]     RETURNS [data: Environment.Byte];      ReadMemoryDataSequence: PROCEDURE [    address: I186.Address, data: LONG POINTER TO  OMF.DataSequence];    ReadMemoryWord: PROCEDURE [address: I186.Address] RETURNS [data: WORD];    ReadSavedRegisters: PROCEDURE;    Stop: PROCEDURE;    WriteMemoryDataSequence: PROCEDURE [address: I186.Address, data: LONG POINTER TO OMF.DataSequence];    WriteMemoryByte: PROCEDURE [address: I186.Address, data: Environment.Byte];    WriteMemoryWord: PROCEDURE [address: I186.Address, data: WORD];    WriteRegister: PROCEDURE [reg: I186.Register, data: WORD];    WriteSavedRegisters: PROCEDURE;    END.