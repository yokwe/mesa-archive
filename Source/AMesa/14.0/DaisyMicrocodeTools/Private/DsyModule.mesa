PUBLIC PROCEDURE [str: LONG STRING] RETURNS [m: MEntryPtr];   InitMTHeap: PROCEDURE [initPages, incrPages: CARDINAL];  InsertM: PUBLIC PROCEDURE [m: MEntryPtr] RETURNS [error: BOOLEAN];   MTtempHeap: UNCOUNTED ZONE;  ReleaseMTSpace: PROCEDURE;    -- from DsyPass1Impl  Pass1: PROCEDURE[];     -- from DsyPhraseImpl  BreakPhraseArg: PROCEDURE [MyS: MyStringPtr] RETURNS [Flags: Flagword];  BreakPhraseMain: PROCEDURE [MyS: MyStringPtr] RETURNS [Flags: Flagword];  SkipUntil: PROCEDURE [str: LONG STRING];    -- from DsyPass2AImpl  Pass2: PROCEDURE[];     -- from DsySTInitImpl  InitMacroTable: PROCEDURE;  --from DsyTempSpaceImpl     SaveArgs: PROCEDURE [    arrayP: LONG POINTER TO ARRAY [0..sizeArgArray] OF LONG STRING,    num: [0..sizeArgArray]];  END...--Log of last edits:--JAC   	 5-Nov-84 14:22:22	fixed arrows for Mesa 11.1