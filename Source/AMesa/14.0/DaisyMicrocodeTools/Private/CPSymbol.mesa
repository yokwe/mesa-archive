AN, s: Symbol];    VirturalToRealCS: PROCEDURE [VirturalCS] RETURNS [CPDef.RealCS, CPDef.IBDisp, CPDef.MDR];  nullVirturalCS: VirturalCS = LAST[VirturalCS];  RealToVirturalCS: PROCEDURE [CPDef.RealCS] RETURNS [VirturalCS, CPDef.IBDisp, CPDef.MDR];    nullRealCS: CPDef.RealCS = LAST[CPDef.RealCS];      AppendSymbol: PROCEDURE [LONG STRING, Symbol];  AppendNumber: PROCEDURE [LONG STRING, UNSPECIFIED];  AppendLongNumber: PROCEDURE [LONG STRING, LONG UNSPECIFIED];  AppendLink: PROCEDURE [LONG STRING, CPDef.Link];  AppendRealCS: PROCEDURE [LONG STRING, CPDef.RealCS];  AppendVirturalCS: PROCEDURE [LONG STRING, VirturalCS];  AppendRReg: PROCEDURE [LONG STRING, CPDef.RReg];  AppendDReg: PROCEDURE [LONG STRING, CPDef.DReg];  AppendAReg: PROCEDURE [LONG STRING, CPDef.AReg];  AppendMap: PROCEDURE [LONG STRING, CPDef.Map];  AppendReal: PROCEDURE [LONG STRING, CPDef.RealMemory];  AppendVirtural: PROCEDURE [LONG STRING, CPDef.VirturalMemory];  AppendOther: PROCEDURE [LONG STRING, CPDef.OtherReg];    AppendMI: PROCEDURE [LONG STRING, CPuFormat.MI];    AppendAllVirturalCS: PROCEDURE [LONG STRING, VirturalCS];  AppendAllRReg: PROCEDURE [LONG STRING, CPDef.RReg];  AppendAllDReg: PROCEDURE [LONG STRING, CPDef.DReg];  AppendAllAReg: PROCEDURE [LONG STRING, CPDef.AReg];    EnumerateCSSymbols: PROCEDURE [PROCEDURE [LONG STRING, VirturalCS]];  EnumerateRRegSymbols: PROCEDURE [PROCEDURE [LONG STRING, CPDef.RReg]];  EnumerateDRegSymbols: PROCEDURE [PROCEDURE [LONG STRING, CPDef.DReg]];  EnumerateARegSymbols: PROCEDURE [PROCEDURE [LONG STRING, CPDef.AReg]];  EnumerateMap: PROCEDURE [PROCEDURE [CPDef.RealCS, VirturalCS, CPDef.IBDisp, CPDef.MDR]];  END.