G STRING, CPDef.RReg] RETURNS [hit: BOOLEAN];  LookUpDReg: PROCEDURE [LONG STRING, CPDef.DReg] RETURNS [hit: BOOLEAN];  LookUpAReg: PROCEDURE [LONG STRING, CPDef.AReg] RETURNS [hit: BOOLEAN];  LoadPrivateSymbols: PROCEDURE [Format.StringProc, LONG POINTER, LONG STRING];    END.