-- SymToolDefs.mesa  -- Sweet,  1-Jun-84 10:42:09DIRECTORY  FormSW USING [NotifyProcType],  Literals,  PackageSymbols,  RESOut USING [WindowsHandle],  String,  Symbols,  SymbolSegment,  Table;  SymToolDefs: DEFINITIONS =  BEGIN    opType: Table.Selector = SymbolSegment.codeType + 1;  ipType: Table.Selector = opType + 1;  hvType:Table.Selector = ipType + 1;    handle: RESOut.WindowsHandle;  ParamNotify: FormSW.NotifyProcType;    stFile: LONG STRING;  ClearFileSource: PROC;  LoadSymbols: PROC;  PrintHeader: PROC;  PrintOffsets: PROC;  PrintOuterCtx: PROC RETURNS [Symbols.CTXIndex];    PutAsOpi: PROC [PackageSymbols.OPIndex];  PrintMwconst: PROC;  PrintFgtHdr: PROC;  PrintNxOpi: PROC;  PrintLco: PROC;  PutAsFgi: PROC [CARDINAL];  PutAsIpi: PROC [PackageSymbols.IPIndex];  PrintLio: PROC;  PutAsSLti: PROC [Literals.STIndex];  PrintNxFgi: PROC;  PrintNxIpi: PROC;  PrintJibw: PROC;    FindString: PROC [s: String.SubString] RETURNS [hti: Symbols.HTIndex];  SearchContext: PROC [hti: Symbols.HTIndex, ctx: Symbols.CTXIndex]     RETURNS [Symbols.ISEIndex];  END.