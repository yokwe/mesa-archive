ass,    s: STRING, name: BcdDefs.NameRecord, base: BcdOps.NameString];  RefInTwoVersions: PROC [    class: ErrorClass, fileName: BcdDefs.NameRecord, v1, v2: BcdDefs.VersionStamp];  WrongImportVersion: PROC [class: ErrorClass, avail, req: BcdDefs.FTIndex];  WrongExportVersion: PROC [class: ErrorClass, avail, req: BcdDefs.FTIndex];    END.