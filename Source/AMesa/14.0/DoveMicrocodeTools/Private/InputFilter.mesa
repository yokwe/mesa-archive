-- InputFilter.mesa-- JWF 29-Jul-86 16:19:49DIRECTORY     Window,     FormSW;     InputFilter: DEFINITIONS =BEGINMaxFieldLength: CARDINAL = 80;InsertCharType: TYPE = {Fixed, Floating};InsertCharDescType: TYPE = RECORD [   c: CHARACTER,   charType: InsertCharType,   pos: CARDINAL,   ShowIfFirstChar: BOOLEAN];ValidCharProcType: TYPE = PROC[c: CHARACTER] RETURNS[BOOLEAN];FilterProc: PROC[   sw:Window.Handle,   item: FormSW.ItemHandle,   typeIn: CARDINAL,   inputString: LONG STRING,   padChar: CHARACTER,   minLength: CARDINAL,   InsertCharDescArray: DESCRIPTOR FOR ARRAY OF InsertCharDescType,   ValidChar: ValidCharProcType];   END.