-- Definitions for writing and reading state information for Prom Blowing-- File: PromBlowDefs.mesa-- Last edited by PaD  DXS : June 5, 1979  12:13 PM-- Last edited by Roy RXO : July 5, 1979  2:40 PM-- Last edited by Frank FXB      9-Aug-83  9:46:15-- Last edited by Frank FXB    30-Oct-84 13:41:54DIRECTORY  Exec USING [Handle];  PromBlow: DEFINITIONS =BEGINNoPromFile: SIGNAL;WritePromList: PROCEDURE [Name: LONG STRING, addressWidth, dataWidth: CARDINAL, ptr:  LONG POINTER, h: Exec.Handle];WritePromData: PROCEDURE [Name: LONG STRING, memSize, dataWidth: CARDINAL, ptr: LONG POINTER, h: Exec.Handle]; ReadPromData: PROCEDURE [Name: STRING, ptr: LONG POINTER]				RETURNS [dataWidth, NoOfRecs: CARDINAL];END.