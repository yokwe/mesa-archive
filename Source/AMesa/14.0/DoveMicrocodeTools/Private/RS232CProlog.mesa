-- RS232CProlog.mesa-- Interface for communicating with Prolog M980 Prom Programmer-- Last edited by-- Purves   27-Aug-85 17:30:38      moved HexSubStringToLongCardinal, no cursor stuff-- created by Bowers on 12-Jan-84 19:19:20-- Copyright (C) 1985 by Xerox Corporation. All rights reserved.DIRECTORY  Exec USING [Handle],  Window USING [Handle];RS232CProlog: DEFINITIONS =  {    DataLost: SIGNAL;    TransmissionAborted: SIGNAL;    NegativeResponse: SIGNAL;    HexSubStringToCardinal: PROCEDURE [    base: LONG STRING, offset: CARDINAL, length: [0..4]]    RETURNS [n: CARDINAL ¬ 0];      HexSubStringToLongCardinal: PROCEDURE [    base: LONG STRING, offset: CARDINAL, length: [0..8]]    RETURNS [n: LONG CARDINAL ¬ 0];    RS232CInit: PROCEDURE;    TransitionToInactiveState: PROCEDURE;    Initialize: PROCEDURE [h: Exec.Handle];    GiveItUp: PROCEDURE;    CopyPromToProgrammerBuffer: PROCEDURE;    ProgramWholeProm: PROCEDURE;    ReadBlock: PROCEDURE [startAddress: CARDINAL, block: LONG STRING];    WriteBlock: PROCEDURE [block: LONG STRING, startAddress, length: CARDINAL];    InitPromType: PROCEDURE [initCode: CARDINAL, z: UNCOUNTED ZONE];    InitLogHandle: PROCEDURE [logHandle: Window.Handle];    SetDebugFlag: PROCEDURE [debugFlag: BOOLEAN];    AppendHexNumber: PROCEDURE [s: LONG STRING, n, w: CARDINAL];    IntelFormat: PROCEDURE [address: CARDINAL, data: LONG STRING, cs: CARDINAL, z: UNCOUNTED ZONE]  RETURNS [intelHex: LONG STRING];    log:  Window.Handle;  debug: BOOLEAN;    }.     