-- Umbilical.mesa, HGM, 30-Sep-82  9:26:43-- last edit by Frank FXB    30-Jul-84 16:10:32DIRECTORY  Environment USING [Byte];Umbilical: DEFINITIONS =  BEGIN    Timeout: ERROR;    UmbilicalNotWorking: ERROR;  Boot: PROCEDURE;  -- Beware: Word transfers are low order byte first.  WriteByte: PROCEDURE [Environment.Byte];  WriteWord: PROCEDURE [WORD];  WriteBlock: PROCEDURE [    dataPtr: LONG POINTER TO PACKED ARRAY INTEGER[0..0) OF Environment.Byte,     size: Environment.Byte];    ReadReady: PROCEDURE RETURNS [BOOLEAN];  ReadByte: PROCEDURE RETURNS [Environment.Byte];  ReadWord: PROCEDURE RETURNS [WORD];  ReadBlock: PROCEDURE [    dataPtr: LONG POINTER TO PACKED ARRAY[0..0) OF Environment.Byte,    size: Environment.Byte];          ToggleNMI: PROCEDURE;      END.