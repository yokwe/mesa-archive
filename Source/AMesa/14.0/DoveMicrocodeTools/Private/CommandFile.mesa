-- CommandFile.mesa, HGM,  7-Feb-83 13:45:10DIRECTORY  Format USING [StringProc],  Stream USING [Handle, Position];CommandFile: DEFINITIONS =  BEGIN    Error: ERROR [reason: LONG STRING, position: Stream.Position];    AbortProc: TYPE = PROCEDURE RETURNS [BOOLEAN];    SomebodyRunning: PROCEDURE RETURNS [BOOLEAN];  CommandsFromFile: PROCEDURE [Format.StringProc, LONG POINTER, AbortProc, LONG STRING];  CommandsFromStream: PROCEDURE [Format.StringProc, LONG POINTER, AbortProc, Stream.Handle];  END.