---- SymbolHash.mesa, AYC   , 30-Apr-84 11:20:24-- From SymHash.mesa, HGM, 12-Nov-83 22:37:47-- From SimHashDefs of ETN  , PXO     , PXJ   --SymbolHash: DEFINITIONS =  BEGIN    Handle: TYPE [2];  nullHandle: Handle = LOOPHOLE[LONG[NIL]];  Hash: TYPE [2];  nullHash: Hash = LOOPHOLE[LONG[NIL]];    Lookup: PROCEDURE [Handle, LONG STRING] RETURNS [Hash];  Insert: PROCEDURE [Handle, LONG STRING] RETURNS [Hash];  PutInfo: PROCEDURE [Handle, Hash, LONG UNSPECIFIED];  GetInfo: PROCEDURE [Handle, Hash] RETURNS [LONG UNSPECIFIED];  GetString: PROCEDURE [Handle, Hash] RETURNS [LONG STRING];  Create: PROCEDURE [pages: CARDINAL] RETURNS [Handle];  Reset: PROCEDURE [Handle];  Destroy: PROCEDURE [Handle];  EnumerateStrings: PROCEDURE [Handle, PROCEDURE [LONG STRING]];  EnumerateEntries: PROCEDURE [Handle, PROCEDURE [Hash]];END....