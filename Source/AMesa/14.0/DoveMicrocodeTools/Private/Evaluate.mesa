--Evaluate.mesa, HGM, 29-Jun-82 15:33:22DIRECTORY  String USING [SubString];Evaluate: DEFINITIONS =  BEGIN    Error: ERROR [reason: LONG STRING];  Type: TYPE = CARDINAL;  unspecified: Type = LAST[Type];  Value: TYPE = LONG CARDINAL;    LookupProcType: TYPE = PROCEDURE [LONG STRING] RETURNS [Type, Value];    EvaluateString: PROCEDURE [LONG STRING, LookupProcType] RETURNS [Type, Value];  EvaluateSubString: PROCEDURE [String.SubString, LookupProcType] RETURNS [Type, Value];  END.