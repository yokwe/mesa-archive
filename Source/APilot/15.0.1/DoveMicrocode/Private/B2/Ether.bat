REM Ether.batREM Copyright (C) 1987 by Xerox Corporation. All rights reserved.BREAK ONREM ----------------------REM Build Ether.lnk!REM ----------------------XFILE RET IOPStack.asm -oXFILE RET IOPDefs.asm -oXFILE RET QueDefs.asm -oXFILE RET IOPMacro.asm -oXFILE RET HardDefs.asm -oXFILE RET QueMacro.asm -oXFILE RET EtherTsk.asm -oXFILE RET EthHdFce.asm -oXFILE RET EthIOFce.asm -oXFILE RET IOREther.asm -oXFILE RET STKEther.asm -oCRLFTool /lf IOPStack.asmDEL IOPStack.bakCRLFTool /lf IOPDefs.asmDEL IOPDefs.bakCRLFTool /lf QueDefs.asmDEL QueDefs.bakCRLFTool /lf IOPMacro.asmDEL IOPMacro.bakCRLFTool /lf HardDefs.asmDEL HardDefs.bakCRLFTool /lf QueMacro.asmDEL QueMacro.bakCRLFTool /lf EtherTsk.asmDEL EtherTsk.bakCRLFTool /lf EthHdFce.asmDEL EthHdFce.bakCRLFTool /lf EthIOFce.asmDEL EthIOFce.bakCRLFTool /lf IOREther.asmDEL IOREther.bakCRLFTool /lf STKEther.asmDEL STKEther.bakRUN ASM86 EtherTsk.asm DEBUG > EtherTsk.logRUN ASM86 IOREther.asm DEBUG > IOREther.logRUN ASM86 STKEther.asm DEBUG > STKEther.logTYPE EtherTsk.logTYPE IOREther.logTYPE STKEther.logPAUSERUN LINK86 EtherTsk.obj,IOREther.obj,STKEther.obj to Ether.lnkXFILE STORE EtherTsk.objXFILE STORE IOREther.objXFILE STORE STKEther.objXFILE STORE Ether.lnkXFILE STORE Ether.mp1REM RAMDaisy.bat AND ROMRAMSys.bat (Daybreak) depend on components rebuilt in here!REM fini