REM Beep.batREM Copyright (C) 1986 by Xerox Corporation. All rights reserved.BREAK ONREM -----------------------REM Build BepHndlr.lnk!REM -----------------------XFILE RET IOPDefs.asm -oXFILE RET IOPStack.asm -oXFILE RET IOPMacro.asm -oXFILE RET HardDefs.asm -oXFILE RET IORBeep.asm -oXFILE RET Beep.asm -oXFILE RET STKBeep.asm -oCRLFTool /lf IOPDefs.asmDEL IOPDefs.bakCRLFTool /lf IOPStack.asmDEL IOPStack.bakCRLFTool /lf IOPMacro.asmDEL IOPMacro.bakCRLFTool /lf HardDefs.asmDEL HardDefs.bakCRLFTool /lf IORBeep.asmDEL IORBeep.bakCRLFTool /lf Beep.asmDEL Beep.bakCRLFTool /lf STKBeep.asmDEL STKBeep.bakRUN ASM86 IORBeep.asm DEBUG > IORBeep.logRUN ASM86 Beep.asm DEBUG > Beep.logRUN ASM86 STKBeep.asm DEBUG > STKBeep.logTYPE IORBeep.logTYPE Beep.logTYPE STKBeep.logPAUSERUN LINK86 Beep.obj,IORBeep.obj,STKBeep.obj to BepHndlr.lnkXFILE STORE IORBeep.obj -oXFILE STORE Beep.obj -oXFILE STORE STKBeep.obj -oXFILE STORE BepHndlr.lnk -oXFILE STORE BepHndlr.mp1 -oREM RAMDaisy.bat AND ROMRAMSys.bat (Daybreak) depend on components rebuilt in here!REM fini