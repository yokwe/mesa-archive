REM Floppy.bat (Daybreak)REM Copyright (C) 1987 by Xerox Corporation. All rights reserved.BREAK ONREM ----------------------REM Build Floppy.lnk!REM ----------------------XFILE RET IOPStack.asm -oXFILE RET IOPDefs.asm -oXFILE RET QueDefs.asm -oXFILE RET IOPMacro.asm -oXFILE RET HardDefs.asm -oXFILE RET ROMEEP.asm -oXFILE RET QueMacro.asm -oXFILE RET FlopFace.asm -oXFILE RET FlopHand.asm -oXFILE RET STKFlop.asm -oXFILE RET IORFlop.asm -oCRLFTool /lf IOPStack.asmDEL IOPStack.bakCRLFTool /lf IOPDefs.asmDEL IOPDefs.bakCRLFTool /lf QueDefs.asmDEL QueDefs.bakCRLFTool /lf IOPMacro.asmDEL IOPMacro.bakCRLFTool /lf HardDefs.asmDEL HardDefs.bakCRLFTool /lf ROMEEP.asmDEL ROMEEP.bakCRLFTool /lf QueMacro.asmDEL QueMacro.bakCRLFTool /lf FlopFace.asmDEL FlopFace.bakCRLFTool /lf FlopHand.asmDEL FlopHand.bakCRLFTool /lf STKFlop.asmDEL STKFlop.bakCRLFTool /lf IORFlop.asmDEL IORFlop.bakRUN ASM86 FlopHand.asm DEBUG > FlopHand.logRUN ASM86 STKFlop.asm DEBUG > STKFlop.logRUN ASM86 IORFlop.asm DEBUG > IORFlop.logTYPE FlopHand.logTYPE STKFlop.logTYPE IORFlop.logPAUSERUN LINK86 FlopHand.obj,IORFlop.obj,STKFlop.obj to Floppy.lnkXFILE STORE FlopHand.obj -oXFILE STORE STKFlop.obj -oXFILE STORE IORFlop.obj -oXFILE STORE Floppy.lnk -oXFILE STORE Floppy.mp1 -oREM ROMRAMSys.bat (Daybreak) depends on components rebuilt in here!REM fini