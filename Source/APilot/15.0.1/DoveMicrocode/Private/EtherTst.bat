REM EtherTst.batREM uses bootstrap's handler slot.REM including the display IOR just quiets the unresolved's.XFILE RET pEtherTst.asm -o -n=EtherTst.asmXFILE RET pEchoInit.asm -o -n=EchoInit.asmXFILE RET pIORETest.asm -o -n=IORETest.asmXFILE RET pSTKETest.asm -o -n=STKETest.asmXFILE RET pETestLoc.bat -o -n=ETestLoc.batxfile ret IORDisp.obj -oRUN ASM86  EtherTst.asm DEBUGRUN ASM86  EchoInit.asm DEBUGRUN ASM86  IORETest.asm DEBUGRUN ASM86  STKETest.asm DEBUGPAUSERUN LINK86 EtherTst.obj,IORETest.obj, STKETest.obj to Temp.lnkRUN LINK86 OpieDaze.lnk, IORDisp.obj,Ether.lnk,Temp.lnk,EchoInit.obj,HandInit.obj to EtherTst.lnkDEL TEMP.*REM ETestLoc = PC(Purge) START(Initialization) &REM           ADDRESSES (SEGMENTS(IOPEINROM(08000H),&REM           IOREGION(0BFC0H),&REM           OPIESTK(0580H),&REM           ETHERNETSTK(0700H),&REM           BOOTSTRAPSTK(07F0H),&REM           OPIEIOR(0890H),&REM           ETHERNETIOR(0A60H),&REM           BOOTSTRAPIOR(1600H)))RUN LOC86 EtherTst.lnk to EtherTst.loc & < ETestLoc.batXFILE STORE EtherTst.objXFILE STORE IORETest.objXFILE STORE STKETest.objXFILE STORE EchoInit.objXFILE STORE EtherTst.lnkXFILE STORE EtherTst.locXFILE STORE EtherTst.mp2