REM AllocSgs.batREM Copyright (C) 1987 by Xerox Corporation. All rights reserved.BREAK ONREM ----------------------REM Build STKSegs.lnk!REM ----------------------XFILE RET STKOpie.obj -oXFILE RET STKBeep.obj -oXFILE RET STKDisk.obj -oXFILE RET STKDisp.obj -oXFILE RET STKEther.obj -oXFILE RET STKFlop.obj -oXFILE RET STKKeyMo.obj -oXFILE RET STKMaint.obj -oXFILE RET STKMesa.obj -oXFILE RET STKPrty.obj -oXFILE RET STKBerm.obj -oXFILE RET STKTTY.obj -oXFILE RET STKRS232.obj -oXFILE RET STKCnfig.obj -oXFILE RET STKPCE.lnk -oREM XFILE RET STKPSO.obj -oXFILE RET STKBoot.obj -oXFILE RET STKMmPCE.lnk -oXFILE RET STKSys.obj -oXFILE RET STKPPO.obj -oXFILE RET STKEnd.asm -oCRLFTool /lf STKEnd.asmDEL STKEnd.bakRUN ASM86 STKEnd.asm DEBUG > STKEnd.logTYPE STKEnd.logPAUSERUN LINK86 STKOpie.obj,STKBeep.obj,STKDisp.obj,STKKeyMo.obj,STKMaint.obj,STKBoot.obj,STKDisk.obj,STKSys.obj TO Temp1.lnkRUN LINK86 Temp1.lnk,STKEther.obj,STKFlop.obj,STKMesa.obj,STKTTY.obj,STKPrty.obj,STKCnfig.obj,STKRS232.obj TO Temp2.lnkRUN LINK86 Temp2.lnk,STKPCE.lnk,STKMmPCE.lnk,STKPPO.obj,STKBerm.obj,STKEnd.obj TO STKSegs.lnkXFILE STORE STKEnd.obj -oXFILE STORE STKSegs.lnk -oXFILE STORE STKSegs.mp1 -oREM ----------------------REM Build IORSegs.lnk!REM ----------------------XFILE RET IORegion.obj -oXFILE RET IOROpie.obj -oXFILE RET IOROpieX.obj -oXFILE RET IORBeep.obj -oXFILE RET IORDisk.obj -oXFILE RET IORDisp.obj -oXFILE RET IORMaint.obj -oXFILE RET IOREther.obj -oXFILE RET IORFlop.obj -oXFILE RET IORKeyMo.obj -oXFILE RET IORPrty.obj -oXFILE RET IORMesa.obj -oXFILE RET IORuCode.obj -oXFILE RET IORTTY.obj -oXFILE RET IORRS232.obj -oXFILE RET IORCnfig.obj -oXFILE RET IORPCE.lnk -oREM XFILE RET IORPSO.obj -oXFILE RET IORBerm.obj -oXFILE RET IORRAMBt.obj -oXFILE RET IORMmPCE.lnk -oXFILE RET IORPPO.obj -oXFILE RET IOREnd.asm -oCRLFTool /lf IOREnd.asmDEL IOREnd.bakRUN ASM86 IOREnd.asm DEBUG > IOREnd.logTYPE IOREnd.logPAUSERUN LINK86 IORegion.obj,IOROpie.obj,IOROpieX.obj,IORBeep.obj,IORDisk.obj,IORDisp.obj,IOREther.obj,IORFlop.obj TO Temp2.lnkRUN LINK86 IORKeyMo.obj,IORPrty.obj,IORMaint.obj,IORMesa.obj,IORuCode.obj,IORTTY.obj,IORRS232.obj,IORCnfig.obj TO Temp3.lnkRUN LINK86 Temp2.lnk,Temp3.lnk,IORPCE.lnk,IORMmPCE.lnk,IORPPO.obj,IORBerm.obj,IORRAMBt.obj,IOREnd.obj TO IORSegs.lnkXFILE STORE IOREnd.obj -oXFILE STORE IORSegs.lnk -oXFILE STORE IORSegs.mp1 -oREM -----------------------REM Build AllocSgs.loc!REM -----------------------XFILE RET AllocSgs.asm -oXFILE RET IOPDefs.asm -oXFILE RET OpieDefs.asm -oXFILE RET ROMBDefs.asm -oXFILE RET IOPMacro.asm -oXFILE RET RAMHands.asm -oXFILE RET SegLoc.bat -oXFILE RET IOPLRAM.obj -oCRLFTool /lf AllocSgs.asmDEL AllocSgs.bakCRLFTool /lf IOPDefs.asmDEL IOPDefs.bakCRLFTool /lf OpieDefs.asmDEL OpieDefs.bakCRLFTool /lf ROMBDefs.asmDEL ROMBDefs.bakCRLFTool /lf IOPMacro.asmDEL IOPMacro.bakCRLFTool /lf RAMHands.asmDEL RAMHands.bakCRLFTool /lf SegLoc.batDEL SegLoc.bakRUN ASM86 AllocSgs.asm DEBUG > AllocSgs.logTYPE AllocSgs.logPAUSERUN LINK86 IOPLRAM.obj,STKSegs.lnk,IORSegs.lnk,AllocSgs.obj TO AllocSgs.lnkRUN LOC86 AllocSgs.lnk TO AllocSgs.loc ST(AllocateSegments) &< SegLoc.batXFILE STORE AllocSgs.obj -oXFILE STORE AllocSgs.loc -oXFILE STORE AllocSgs.lnk -oXFILE STORE AllocSgs.mp2 -oREM DazeLoad.bat AND MesaDove.cm depend on components rebuilt in here!REM fini