REM TTY.batREM Copyright (C) 1987 by Xerox Corporation. All rights reserved.BREAK ONREM ----------------------REM Build TTYHndlr.lnk!REM ----------------------XFILE RET RAMEEP.asm -oXFILE RET IOPDefs.asm -oXFILE RET HardDefs.asm -oXFILE RET IOPMacro.asm -oXFILE RET IOPStack.asm -oXFILE RET TTYIOFce.asm -oXFILE RET TTYHdFce.asm -oXFILE RET IORTTY.asm -oXFILE RET STKTTY.asm -oXFILE RET TTYHndlr.asm -oCRLFTool /lf RAMEEP.asmDEL RAMEEP.bakCRLFTool /lf IOPDefs.asmDEL IOPDefs.bakCRLFTool /lf HardDefs.asmDEL HardDefs.bakCRLFTool /lf IOPMacro.asmDEL IOPMacro.bakCRLFTool /lf IOPStack.asmDEL IOPStack.bakCRLFTool /lf TTYIOFce.asmDEL TTYIOFce.bakCRLFTool /lf TTYHdFce.asmDEL TTYHdFce.bakCRLFTool /lf IORTTY.asmDEL IORTTY.bakCRLFTool /lf STKTTY.asmDEL IORTTY.bakCRLFTool /lf TTYHndlr.asmDEL TTYHndlr.bakRUN ASM86 IORTTY.asm DEBUG > IORTTY.logRUN ASM86 STKTTY.asm DEBUG > STKTTY.logRUN ASM86 TTYHndlr.asm DEBUG > TTYHndlr.logTYPE IORTTY.logTYPE STKTTY.logTYPE TTYHndlr.logPAUSERUN LINK86 IORTTY.obj,STKTTY.obj,TTYHndlr.obj to TTYHndlr.lnkXFILE STORE IORTTY.obj -oXFILE STORE STKTTY.obj -oXFILE STORE TTYHndlr.obj -oXFILE STORE TTYHndlr.lnk -oXFILE STORE TTYHndlr.mp1 -oREM DazeLoad.bat depends on components rebuilt in here!REM fini