REM DoveCP.batREM Copyright (C) 1987 by Xerox Corporation. All rights reserved.BREAK ONREM ----------------------REM Build DoveCP.loc!REM ----------------------REM This batch file is the concatenation of GetDvCP.bat, AsmDvCP.bat, and PutDvCP.bat.XFILE RET IORRAMBt.obj -oXFILE RET IOPLRAM.obj -oXFILE RET DaisyCP.asm -oXFILE RET DybrkCP.asm -oXFILE RET DoveCP.asm -oXFILE RET HardDefs.asm -oXFILE RET IOPDefs.asm -oXFILE RET IOPMacro.asm -oXFILE RET RAMBDefs.asm -oXFILE RET CSBankDf.asm -oXFILE RET RAMEEP.asm -oCRLFTool /lf DaisyCP.asmDEL DaisyCP.bakCRLFTool /lf DybrkCP.asmDEL DybrkCP.bakCRLFTool /lf DoveCP.asmDEL DoveCP.bakCRLFTool /lf HardDefs.asmDEL HardDefs.bakCRLFTool /lf IOPDefs.asmDEL IOPDefs.bakCRLFTool /lf IOPMacro.asmDEL IOPMacro.bakCRLFTool /lf RAMBDefs.asmDEL RAMBDefs.bakCRLFTool /lf CSBankDf.asmDEL CSBankDf.bakCRLFTool /lf RAMEEP.asmDEL RAMEEP.bakRUN ASM86 DaisyCP.asm DEBUG > DaisyCP.logRUN ASM86 DybrkCP.asm DEBUG > DybrkCP.logRUN ASM86 DoveCP.asm DEBUG > DoveCP.logTYPE DaisyCP.logTYPE DybrkCP.logTYPE DoveCP.logPAUSERUN LINK86 DoveCP.obj, DaisyCP.obj, DybrkCP.obj, IORRAMBt.obj, IOPLRAM.obj TO DoveCP.lnkREM Expect a warning about IOPELOCALRAM overlapping IOPEINRAM.  It is ok.RUN LOC86 DoveCP.lnk TO DoveCP.loc PC(PURGE) START(InitCPSpecific) ADDRESSES(SEGMENTS(IOPEINRAM(0H)))REM Expect a warning about IOPELOCALRAM overlapping IOPEINRAM.  It is ok.XFILE STORE DoveCP.obj -oXFILE STORE DaisyCP.obj -oXFILE STORE DybrkCP.obj -oXFILE STORE DoveCP.lnk -oXFILE STORE DoveCP.loc -oXFILE STORE DoveCP.mp2 -oREM MesaDove.cm depends on components rebuilt in here!REM fini