REM KyMoInit.batREM Copyright (C) 1987 by Xerox Corporation. All rights reserved.BREAK ONREM ----------------------REM Build KyMoInit.loc!REM ----------------------XFILE RET KyMoInit.asm -oXFILE RET IOPDefs.asm -oXFILE RET IOPMacro.asm -oXFILE RET RAMHands.asm -oXFILE RET IORKeyMo.obj -oCRLFTool /lf KyMoInit.asmDEL KyMoInit.bakCRLFTool /lf IOPDefs.asmDEL IOPDefs.bakCRLFTool /lf IOPMacro.asmDEL IOPMacro.bakCRLFTool /lf RAMHands.asmDEL RAMHands.bakCRLFTool /lf IORKeyMo.asmDEL IORKeyMo.bakRUN ASM86 KyMoInit.asm DEBUG > KyMoInit.logTYPE KyMoInit.logPAUSERUN LINK86 IORKeyMo.obj,KyMoInit.obj to KyMoInit.lnkRUN LOC86 KyMoInit.lnk to KyMoInit.loc START(KyMoInitProc) ADDRESSES(SEGMENTS(IOPEInRAM(0H)))XFILE STORE KyMoInit.obj -oXFILE STORE KyMoInit.lnk -oXFILE STORE KyMoInit.loc -oXFILE STORE KyMoInit.mp2 -oREM MesaDove.cm depends on components rebuilt in here!REM fini