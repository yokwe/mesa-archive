REM MesaVM.bat REM Copyright (C) 1987 by Xerox Corporation. All rights reserved.BREAK ONREM ----------------------REM Build MesaVM.loc!REM ----------------------XFILE RET HardDefs.asm -oXFILE RET IOPDefs.asm -oXFILE RET ROMEEP.asm -oXFILE RET RAMEEP.asm -oXFILE RET IOPMacro.asm -oXFILE RET VMMDefs.asm -oXFILE RET MesaVM.asm -oXFILE RET IOPLRAM.obj -oXFILE RET IORRAMBt.obj -oXFILE RET IOROpie.obj -oXFILE RET IORDisp.obj -oCRLFTool /lf HardDefs.asmDEL HardDefs.bakCRLFTool /lf IOPDefs.asmDEL IOPDefs.bakCRLFTool /lf ROMEEP.asmDEL ROMEEP.bakCRLFTool /lf RAMEEP.asmDEL RAMEEP.bakCRLFTool /lf IOPMacro.asmDEL IOPMacro.bakCRLFTool /lf VMMDefs.asmDEL VMMDefs.bakCRLFTool /lf MesaVM.asmDEL MesaVM.bakRUN ASM86 MesaVM.asm DEBUG > MesaVM.logTYPE MesaVM.logPAUSERUN LINK86 IOPLRAM.obj,IORRAMBt.obj,IOROpie.obj,IORDisp.obj,MesaVM.obj to MesaVM.lnkRUN LOC86 MesaVM.lnk to MesaVM.loc PC(PURGE) START(MesaVM) ADDRESSES(SEGMENTS (IOPEInRAM(0H)))XFILE STORE MesaVM.mp2 -oXFILE STORE MesaVM.loc -oXFILE STORE MesaVM.lnk -oXFILE STORE MesaVM.obj -oREM MesaDove.cm depend on components rebuilt in here!REM fini