REM Parity.batREM Copyright (C) 1987 by Xerox Corporation. All rights reserved.BREAK ONREM ----------------------REM Build Parity.lnk!REM ----------------------XFILE RET IOPDefs.asm -oXFILE RET HardDefs.asm -oXFILE RET IOPMacro.asm -oXFILE RET RAMEEP.asm -oXFILE RET IOPStack.asm -oXFILE RET IORPrty.asm -oXFILE RET STKPrty.asm -oXFILE RET PrtyHdlr.asm -oCRLFTool /lf IOPDefs.asmDEL IOPDefs.bakCRLFTool /lf HardDefs.asmDEL HardDefs.bakCRLFTool /lf IOPMacro.asmDEL IOPMacro.bakCRLFTool /lf RAMEEP.asmDEL RAMEEP.bakCRLFTool /lf IOPStack.asmDEL IOPStack.bakCRLFTool /lf IORPrty.asmDEL IORPrty.bakCRLFTool /lf STKPrty.asmDEL STKPrty.bakCRLFTool /lf PrtyHdlr.asmDEL PrtyHdlr.bakRUN ASM86 IORPrty.asm DEBUG > IORPrty.logRUN ASM86 STKPrty.asm DEBUG > STKPrty.logRUN ASM86 PrtyHdlr.asm DEBUG > PrtyHdlr.logTYPE IORPrty.logTYPE STKPrty.logTYPE PrtyHdlr.logPAUSERUN LINK86 IORPrty.obj,STKPrty.obj,PrtyHdlr.obj to Parity.lnkXFILE STORE IORPrty.obj -oXFILE STORE STKPrty.obj -oXFILE STORE PrtyHdlr.obj -oXFILE STORE Parity.lnk -oXFILE STORE Parity.mp1 -oREM DazeLoad.bat depends on components rebuilt in here!REM fini