REM KeyMo.bat (Daybreak)REM Copyright (C) 1987 by Xerox Corporation. All rights reserved.BREAK ONREM ----------------------REM Build KeyHndlr.lnk!REM ----------------------XFILE RET IOPStack.asm -oXFILE RET IOPDefs.asm -oXFILE RET IOPMacro.asm -oXFILE RET HardDefs.asm -oXFILE RET KyMoDefs.asm -oXFILE RET IORKeyMo.asm -oXFILE RET KeyMo.asm -oXFILE RET STKKeyMo.asm -oCRLFTool /lf IOPStack.asmDEL IOPStack.bakCRLFTool /lf IOPDefs.asmDEL IOPDefs.bakCRLFTool /lf IOPMacro.asmDEL IOPMacro.bakCRLFTool /lf HardDefs.asmDEL HardDefs.bakCRLFTool /lf KyMoDefs.asmDEL KyMoDefs.bakCRLFTool /lf IORKeyMo.asmDEL IORKeyMo.bakCRLFTool /lf KeyMo.asmDEL KeyMo.bakCRLFTool /lf STKKeyMo.asmDEL STKKeyMo.bakRUN ASM86 IORKeyMo.asm DEBUG > IORKeyMo.logRUN ASM86 KeyMo.asm DEBUG > KeyMo.logRUN ASM86 STKKeyMo.asm DEBUG > STKKeyMo.logTYPE IORKeyMo.logTYPE KeyMo.logTYPE STKKeyMo.logPAUSERUN LINK86 KeyMo.obj,IORKeyMo.obj,STKKeyMo.obj to KeyHndlr.lnkXFILE STORE IORKeyMo.obj -oXFILE STORE KeyMo.obj -oXFILE STORE STKKeyMo.obj -oXFILE STORE KeyHndlr.lnk -oXFILE STORE KeyHndlr.mp1 -oREM ROMRAMSys.bat (Daybreak) depends on components rebuilt in here!REM fini