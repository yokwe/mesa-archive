E DEPENDENT {notAllowed(0), allowed(1)};  RS232CBootingBit: TYPE = MACHINE DEPENDENT {notAllowed(0), allowed(1)};  DisplayPresentBit: TYPE = MACHINE DEPENDENT {none(0), present(1)};  DisplayBWColorBit: TYPE = MACHINE DEPENDENT {blackAndWhite(0), color(1)};  DisplayMapBit: TYPE = MACHINE DEPENDENT {bitMapType(0), nonBitMapType(1)};  DisplaySizeBit: TYPE = MACHINE DEPENDENT {small(0), large(1)};  KeyboardType: TYPE = MACHINE DEPENDENT { none(0), english(1), european(2), japanese(3), learSiegler(4), (255) };  RigidDiskSectorsPerTrackType: TYPE = [0..256);  RigidDiskHeadsPerCylinderType: TYPE = [0..256);  RigidDiskHeadsCylinderCountType: TYPE = WORD;  RigidDiskReducedWriteCylinderType: TYPE = WORD;  RigidDiskPreCompensationCylinderType: TYPE = WORD;  RigidDiskHeadLandingZoneType: TYPE = WORD;  RigidDiskTypeBits: TYPE = MACHINE DEPENDENT    {none(0), seagateST212(1), tandonTM703(2), tandonTM702AT(3),     seagateST4026(4), quantumQ540(5),     quantumQ530(6), micropolis1303(7), micropolis1325(8),      seagateST213(9), seagateST225(10), seagateST4051(11), seagateST251(12),     seagateST4085(13), seagateST4096(14), extendedTandonTM703(15),     extendedTandonTM702(16), extendedSeagateST4026(17), extendedQuantumQ530(18),     extendedSeagateST225(19), extendedSeagateST4051(20),     extendedSeagateST251(21),      -- added for JDaisy Rigid Disk     mitsubishiMR533(22), mitsubishiMR535(23),     fujitsuM2225D(24), fujitsuM2227D(25),     fujitsuM2243T(26), necD5146H(27),     mitsubishiMR322US(28), necD3126US(29),      mitsubishiMR535US(30), necD5146HUS(31),      fujitsuM2243AS(32), toshibaMK56FB(33),     -- added for Duke     toshibaMK134FA(34), mitsubishiMR335(35),     mitsubishiMR548(36),     -- added for Helen     swift94355100(37), maxtorXT2190(38),     (255)};  FloppyDiskTypeBits: TYPE = MACHINE DEPENDENT {none(0), kb360(4), kb1200(6), fad5000(8), (15)};  --  --  --RAM dependent Types  --  --  MainMemoryBits: TYPE = MACHINE DEPENDENT {notPresent(0), present(1)};  MainMemoryExpansionBits: TYPE = MACHINE DEPENDENT {none(0), k256(1), k512(2), k768(3), mega1(4),  							  k1256(5), k1512(6), k1768(7), mega2(8),							  k2256(9), k2512(10), k2768(11), mega3(12), (255) };  VMSizeBits: TYPE = MACHINE DEPENDENT {none(0), vm8mega(1), vm16mega(2),  vm32mega(3), (15) };  ControlStoreSizeBits: TYPE = MACHINE DEPENDENT {none(0), cs4k(1), cs8k(2), cs16k(4), (15) };  HardwareVersionBits: TYPE = MACHINE DEPENDENT {none(0), B0B1(1), B2(2), (255) };  DefaultBootDiagBit: TYPE = MACHINE DEPENDENT {noDiags(0), runDiags(1)};  DefaultDiagnosticsBit: TYPE = MACHINE DEPENDENT {normal(0), fast(1)};  DCEdeviceTypeBits: TYPE = MACHINE DEPENDENT {none(0), present(1)};  DCEattributesBits: TYPE = [0..256);  DTEdeviceTypeBits: TYPE = MACHINE DEPENDENT {none(0), present(1)};  DTEattributesBits: TYPE = [0..256);  PCOptionBoardBits: TYPE = [0..256);  OptionType: TYPE = MACHINE DEPENDENT     {none(0), laserPrintingOption(1), printingScanningOption(2), (31)};  ExpansionInterruptAllocation: TYPE =    PACKED ARRAY ExpansionInterruptRequestLineNumber OF BOOLEAN ¬ ALL[FALSE];  ExpansionInterruptRequestLineNumber: TYPE =    {ir0, ir1, ir2, ir3, ir4, ir5, ir6, ir7};  OptionData: TYPE = MACHINE DEPENDENT RECORD [    optionType(0:00..04): OptionType,    optionSpecificData(0:05..07): [0..8),    expansionInterruptAllocation(0:08..15): ExpansionInterruptAllocation    ];        EEpromConfig: TYPE = MACHINE DEPENDENT RECORD [    -- This is where the EEprom configuration is defined for the Mesa world.    -- This configuration must also be reflected in the Opie/Handler world.        VersionEProm (0): VersionNumber,    VersionRAM  (1): VersionNumber,    VersionBadPages (2): VersionNumber,        BootDevice (3:12..15): DefaultBootDevice,    DefaultIcons (3:11..11): DefaultIconBit,    RigidBooting (3:10..10): RigidBootingBit,    FloppyBooting (3:9..9): FloppyBootingBit,    EthrenetBooting (3:8..8): EthrenetBootingBit,    RS232CBooting (3:7..7): RS232CBootingBit,    BootingSpare (3:0..6): [0..128),    DisplayPresent (4:15..15): DisplayPresentBit,    DisplayBWColor (4:14..14): DisplayBWColorBit,    DisplaySize (4:13..13): DisplaySizeBit,    unused4 (4:9..12): [0..16),    DisplayMap (4:8..8): DisplayMapBit,    DisplayxAlignment (4:0..7): [0..256),    DisplayyAlignment (5:8..15): [0..256),    Keyboard (5:0..7): KeyboardType,    RigidDiskSectorsPerTrack (6:8..15): RigidDiskSectorsPerTrackType,    RigidDiskHeadsPerCylinder (6:0..7): RigidDiskHeadsPerCylinderType,    RigidDiskHeadsCylinderCount (7): RigidDiskHeadsCylinderCountType,    RigidDiskReducedWriteCylinder(8): RigidDiskReducedWriteCylinderType,    RigidDiskPreCompensationCylinder (9): RigidDiskPreCompensationCylinderType,    RigidDiskType (10:8..15): RigidDiskTypeBits,    SpareRomRam1 (10:0..7): [0..256),        MainMemory0 (11:15..15): MainMemoryBits,    MainMemory1 (11:14..14): MainMemoryBits,    MainMemory2 (11:13..13): MainMemoryBits,    MainMemory3 (11:12..12): MainMemoryBits,    MainMemory4 (11:11..11): MainMemoryBits,    MainMemory5 (11:10..10): MainMemoryBits,    MainMemory6 (11:9..9): MainMemoryBits,    MainMemory7 (11:8..8): MainMemoryBits,    MainMemory8 (11:7..7): MainMemoryBits,    MainMemory9 (11:6..6): MainMemoryBits,    MainMemory10 (11:5..5): MainMemoryBits,    MainMemory11 (11:4..4): MainMemoryBits,    MainMemory12 (11:3..3): MainMemoryBits,    MainMemory13 (11:2..2): MainMemoryBits,    MainMemory14 (11:1..1): MainMemoryBits,    MainMemory15 (11:0..0): MainMemoryBits,    MainMemoryExpansion (12:8..15): MainMemoryExpansionBits,    VMMSizeInPages (12:4..7): VMSizeBits,    ControlStoreSize (12:0..3): ControlStoreSizeBits,    HardwareVersion (13:8..15): HardwareVersionBits,    DefaultBootDiag (13:7..7): DefaultBootDiagBit,    DefaultDiagnostics (13:6..6): DefaultDiagnosticsBit,    DefaultSpares (13:0..5): [0..64),    DCEdeviceType (14:8..15): DCEdeviceTypeBits,    DCEattributes (14:0..7): DCEattributesBits,    DTEdeviceType (15:8..15): DTEdeviceTypeBits,    DTEattributes (15:0..7): DTEattributesBits,    PCOptionBoard (16:8..15): PCOptionBoardBits,    PCOptionHardware1 (16:0..7): [0..256),    PCOptionHardware2 (17): WORD,    OptionBoardOne (18): OptionData,    OptionBoardTwo (19): OptionData,    OptionBoardThree (20): OptionData,    FloppyDiskTypeDrive0 (21:0..3): FloppyDiskTypeBits,    FloppyDiskTypeDrive1 (21:4..7): FloppyDiskTypeBits,    FloppyDiskTypeDrive2 (21:8..11): FloppyDiskTypeBits,    FloppyDiskTypeDrive3 (21:12..15): FloppyDiskTypeBits,    BootDiagsForManufacturingTests (22): WORD,    RigidDiskHeadLandingZone (23): RigidDiskHeadLandingZoneType,    NumberOfEEPROMWrites (24): WORD,    SpareRomRam5 (25): WORD,    SpareRomRam6 (26): WORD,    SpareRomRam7 (27): WORD,    SpareRomRam8 (28): WORD,    SpareRomRam9 (29): WORD,    SpareRomRam10 (30): WORD,    SpareRomRam11 (31): WORD,    SpareRomRam12 (32): WORD,    SpareRomRam13 (33): WORD,        AddressLastParity (34:0..31): ARRAY [0..2) OF WORD,	--[0..4294967296),    BadPages (36:0..415): ARRAY [0..26) OF WORD,        EEpromCheckSum (62): WORD,    EEpromInvertedCheckSum (63): WORD ];END.