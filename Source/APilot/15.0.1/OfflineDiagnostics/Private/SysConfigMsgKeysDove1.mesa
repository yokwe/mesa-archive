RRAY SysConfigMessages OF LONG STRING;        SysConfigMessages: TYPE = {  -- Table of enumerated indices for messages.	checkSumOptions,	checkSumOverrideTRUE,	checkSumOverrideFALSE,    	--Messages needed by SysConfigUserImpl	enterResponse,	floppyUser,	floppyUserMsg,	cartridgeUser,	etherUser,	etherUserMsg,	PCOUser,	PCOUserMsg,	LPOptionUser,	diagAtTimeoutMsg,	diagAtTimeout,	enterYesOrNoOrReturn,	enterYesOrNo,	tooManyOptionsUser,	invalidConfigurationUser,	warningForTooSmallVM1,	warningForTooSmallVM2,	warningForTooLargeVM1,	warningForTooLargeVM2,	doYouWishToContinue,	--Messages needed by SysConfigUserKeyboardImpl	askRigDisk,	Mb101,	Mb102,	Mb201,	Mb202,	Mb203,	Mb204,	Mb205,	Mb206,	Mb20A,	Mb20B,	Mb20C,	Mb20D,	Mb401,	Mb402,	Mb403,	Mb404,	Mb405,	Mb406,	Mb40C,	Mb40D,	Mb801,	Mb802,	Mb803,	Mb804,	referRigid,	Mb101Displ,	Mb102Displ,	Mb201Displ,	Mb202Displ,	Mb203Displ,	Mb204Displ,	Mb205Displ,	Mb206Displ,	Mb20ADispl,	Mb20BDispl,	Mb20CDispl,	Mb20DDispl,	Mb401Displ,	Mb402Displ,	Mb403Displ,	Mb404Displ,	Mb405Displ,	Mb406Displ,	Mb40CDispl,	Mb40DDispl,	Mb801Displ,	Mb802Displ,	Mb803Displ,	Mb804Displ,		askKeyboard,	usKeyboard,		askMemory,	userDefaultMemory,	userMemory1,	userMemory2,	userMemory3,	userMemory4,	userMemory5,	referMemory,		defaultMem,	mem1,	mem2,	mem3,	mem4,	mem5,	askUniversal,	askVMSize,	twentyTwoBitsVM,	twentyThreeBitsVM,	twentyFourBitsVM,	questionUser1,	confirmUser1,	questionUser2,	confirmUser2,	userEEPromUpdated,	userEEPromUpdateFailed,	userEEPromAborted,	overRideConfirm,	versionNumberOverride,	unusedNumberOverride,	checksumOverride,		badVersionNumber,		currentSetting,	showConfigHeader,		--Messages needed by SysConfigUserShowAllImpl	keyBoard,	disk,	memory,	floppyDrive,	cartridgetape,	etherOption,	pcOption,	universalOption,	runDiagAtTimeout,	yes,	no,	smallBlackWhiteBitMapped,	smallBlackWhiteNotBitMapped,	largeBlackWhiteBitMapped,	largeB