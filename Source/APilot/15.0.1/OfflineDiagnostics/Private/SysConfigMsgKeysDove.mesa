Messages: TYPE = {  -- Table of enumerated indices for messages.    	topLevelMenuTitle,	readSysConfig,	readSysConfigExpl,	verifyEEPROMCheckSum,	checkSumExpl,	writeEEPromImage,	configDisplay,	configBoot,	configVMControlStore,	configKeyboard,	configMainMemory,	configDefaultBoot,	configFloppyDisk,	configOptionBoards,	configEEPromVersion,	configBadMemPages,	configChangeAllEEprom,	configShowAllEEprom,	readModify,	<<enterHexNumber,	newConfiguration,	continueYesOrNo,>>	exitSelection,	respondToPrompt,	goToNextSelection,	returnToTopLevel,	present,	notPresent,	unknown,	none,	allowed,	notAllowed,	currentSetting,	newSetting,		returnToMenu,	blanks,	continue,	configureUser,	configureUserShowAllEEprom,	configureManufactureUser,	defaultConfiguration,	doNotChange,	FixedDataTitle,		-- SysConfigVMControlStoreImpl config messages	vmControlStore,	VMMSizeInPages,	controlStoreK,		vmPages,	controlStore,	vm8mb,	vm16mb,	vm32mb,	control4k,	control8k,		-- Rigid disk	-- Floppy disk	kb360,	kb1200,	fad5000,	driveZero,	driveOne,	driveTwo,	driveThree,	forDriveZero,	forDriveOne,	forDriveTwo,	forDriveThree,--	floppyDiskCapacities,	diskTypesSupported,	seagateST212,	tandonTM703,	tandonTM702AT,	seagateST4026,	quantumQ540,	quantumQ530,	micropolis1303,	micropolis1325,	seagateST213,	seagateST225,	seagateST4051,	seagateST251,	seagateST4096,	extendedTandonTM703,	extendedTandonTM702,	extendedSeagateST4026,	extendedQuantumQ530,	extendedSeagateST225,	extendedSeagateST4051,	extendedSeagateST251,	-- added for JDaisy	mitsubishiMR533,	mitsubishiMR535,	fujitsuM2225D, 	fujitsuM2227D,	fujitsuM2243T,	necD5146H,	newDisk,	<<newDiskExpl,	 selectDiskType,>>		selectRigidDiskMenu,	FXmenu,	USRXmenu,	configRigidDisk,	generalFixedInfoTitle,	diskType,	sectorsPerTrack,	numberOfHeads,	cylindersPerDrive,	reduceWrCurCylinder,	precompCylinder,	landingZone,		--Messages needed by SysConfigBootDeviceImpl	bootChoices,	r