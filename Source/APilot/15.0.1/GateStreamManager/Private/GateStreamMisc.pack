;-- Teletype FilterTeletype: SEGMENT =  BEGINHot: CODE PACK =   BEGIN   GateTeletype[Get, Put, SetSST, SendAttention, WaitAttention,     ENTRY VECTOR];   END;Cold: CODE PACK =   BEGIN   GateTeletype[MAIN, InitTeletypeDriver, Delete, GetStats, GetGlobalStats,   FreePort, AllowAccess, DisallowAccess, WaitUntilIdle, CATCH CODE];   END;END;  -- of Teletype filterQPackage: SEGMENT =BEGIN  Hot: CODE PACK =    BEGIN    QImpl[ENTRY VECTOR, ExtractQueue, EnqueueLong,      DequeueLong, ExtractQueueLong,      GetNextLong, GetNext, Enqueue, Dequeue, EnqueueElement, DequeueElement,      ExtractElement, CompareQData, FreeElement, AllocateElement,       Enumerate, EnumerateLong, EnqueueInternal, EnqueueLongInternal];    END;    Cold: CODE PACK =    BEGIN    QImpl[MAIN, CreateQueue, DeleteQueue, Finalize];    END;    CatchCode: CODE PACK =    BEGIN    QImpl[CATCH CODE];    END;END;  -- QPackage SegmentGateStreamMisc: SEGMENT MERGES GateStreamMgr, RSTransducer, Teletype, QPackage =  BEGIN  RSTransducerHot: CODE PACK = {RSTransducer.Hot};TTYHot: CODE PACK =   BEGIN  Teletype.Hot;  Teletype.Cold;  --See Bishop note below on TTYCold  END;GateStreamMgrCool: CODE PACK =   BEGIN  GateStreamMgr.Cool;  END;QHot: CODE PACK =   BEGIN  QPackage.Hot;  END;QCold: CODE PACK =  BEGIN  QPackage.Cold;  QPackage.CatchCode;  END;GateStreamCold: CODE PACK =   BEGIN  GateStreamMgr.Cold;  RSTransducer.Cold;  END;--TTYCold: CODE PACK = {Teletype.Cold}; Moved to TTYHot since both together < 1 page.  S-A Bishop 10-Nov-82END;-- Frame packsFramesCommon: FRAME PACK =   BEGIN   GateStreamMgrInstance;   RSTransducerInstance;   GateTeletype;		-- Common + TTY < 1 page so....   QImpl;   GateStreamMgr;   END;     