-- GateStreamManager.cf--  Builds GateStream Manager Interfaces and Implementations--   Last edited:   7-Apr-83 17:50:46  By: MuirheadGateStreamManager: SCRIPT =  BEGINcmFileName _ "GateStreamManager.cm";-- The entries in the following strings must be separated by a single space and-- the string must end with a space interfaces _ "CFDStream GateMake QDefs GateStreamInternal GateStreamCourierInternal "; modules _ "GateStreamInstance RSTransducerInstance GateStreamMgrInstance GateStreamMgr GateTeletype QImpl GateStreamCourier GateStreamRemote GateStreamImpl GateStreamService GateStreamRemoteImpl ";DISPLAY[ "Building ", cmFileName, CR];OPEN [cmFileName];IF ASK ["Compile Interfaces", FALSE] THEN  BEGIN  PUTLINE["//Compile Interfaces"];  compFiles _ interfaces;  IF ASK["Compile specific interfaces", FALSE] THEN    compFiles _ USER["Which ones", compFiles];  PUT["Compile/"];    compileSwitchesDevMode _ "be~j~nps~uw~y";    compileSwitchesRelease _ "~bej~nps~uw~y";  PUT[compileSwitchesRelease];  PUTLINE[" ", compFiles];  EOL[];  END;  IF ASK ["Compile Implementations", TRUE] THEN  BEGIN  PUTLINE["//Compile Implementations"];  compFiles _ modules;  IF ASK["Compile specific Implementations", FALSE] THEN    compFiles _ USER["Which ones", compFiles];  PUT["Compile/"];    compileSwitchesDevMode _ "be~j~nps~uw~y";    compileSwitchesRelease _ "~bej~nps~uw~y";  IF ASK["Development Mode", TRUE]    THEN PUT[compileSwitchesDevMode]    ELSE  PUT[compileSwitchesRelease];  PUTLINE[" ", compFiles];  EOL[];  END;    DISPLAY[CR, "Done Building ", cmFileName, CR];END.LOG.Time:  7-Apr-83 17:50:38	By: Muirhead	Action: Created; file fetch and store handled by DF files in Mesa 10.0 (Sierra).