-- File: GateNetMgt.mesa - last edit:-- Danielson.PA         2-Jul-84 14:38:56DIRECTORY  Auth USING [Credentials, Verifier],  GateStream USING [SessionParameterHandle, TransportObject, ErrorReason,    WaitTime],  System USING [GreenwichMeanTime, NetworkAddress];GateNetMgt: DEFINITIONS =  BEGIN    NetMgtRegister: PROCEDURE [new: CallBackProc] RETURNS [old: CallBackProc];    CallBackProc: TYPE = PROCEDURE [     caller: System.NetworkAddress,  -- who made the GAP call     type: {create, createFailure, delete},  -- what happening     timeStarted: System.GreenwichMeanTime,  -- when session started     sessionParameterHandle: GateStream.SessionParameterHandle,     transport: LONG DESCRIPTOR FOR ARRAY OF GateStream.TransportObject,     createTimeout: GateStream.WaitTime,     version: CARDINAL, -- version of GAP being used     credentials: Auth.Credentials,     verifier: Auth.Verifier,     createFailureReason: GateStream.ErrorReason]; -- valid if type=createFailure      END....  LOG 2-Jul-84 14:04:03 BRD Created file.