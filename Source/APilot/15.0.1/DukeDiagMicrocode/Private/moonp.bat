REM MoonP.batREM Copyright (C) 1986 by Xerox Corporation. All rights reserved.BREAK ONREM -----------------------REM Build MoonFP.lm!REM -----------------------XFILE RET MoonFCBP.asm -oXFILE RET MoonP.asm -oXFILE RET MoonPTCB.asm -oXFILE RET MOONPMC.asm -oCRLFTool /lf MoonFCBP.asmDEL MoonFCBP.bakCRLFTool /lf MoonP.asmDEL MoonP.bakCRLFTool /lf MoonPTCB.asmDEL MoonPTCB.bakCRLFTool /lf MOONPMC.asmDEL MOONPMC.bakRUN ASM86 MoonFCBP.asm DEBUG > MoonFCBP.logRUN ASM86 MoonP.asm DEBUG > MoonP.logRUN ASM86 MoonPTCB.asm > MoonPTCB.logRUN ASM86 MOONPMC.asm > MOONPMC.logTYPE MoonFCBP.logTYPE MoonP.logTYPE MoonPTCB.logTYPE MOONPMC.logPAUSERUN LINK86 MoonF1.lnk,MoonFCBP.obj,MoonP.obj,MOONPMC.obj,MoonPTCB.obj TO MoonFP.lnkRUN LOC86 MoonFP.lnk AD(SM(MonitorCode(600h))) TO MoonFP.lm ST(StartB) PC(PURGE)XFILE STORE MoonFP.loc -oXFILE STORE MoonFP.mp2 -oREM fini