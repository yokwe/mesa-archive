{File name:  moonmemsun.df Description: defs for Main Memory Microcode Test. Created: 16 Oct 84, after R. Garner's 1981 sunlight.df,   as used and possibly edited by J. Katsiroumbas}MacroDef[AlwaysIBDisp, (IBDisp, IBPtr_1)];RegDef[R0,R,0], RegDef[rhR0,RH,0];      {PageWord/Bank address reg used.}RegDef[R1,R,1], RegDef[rhR1,RH,1];      {PageWord/Bank address reg used.}RegDef[R2,R,2], RegDef[rhR2,RH,2];      {Last PageWord/Bank address reg. used}RegDef[R3,R,3], RegDef[rhR3,RH,3];	{gen reg used by subroutines}RegDef[R4,R,4], RegDef[rhR4,RH,4];	{gen reg used by subroutines}RegDef[R5,R,5], RegDef[rhR5,RH,5];	{gen reg used by subroutines}RegDef[DataSP,R,6];	 {Data Start patt. this Pass}RegDef[DataModifier,R,7];RegDef[DataTP,R,8];      {Data Type this pass}RegDef[DataE,R,9];       {current and ending Data Pattern}RegDef[PageWordS,R,0A]; {Starting Page and Word address in use by the test ->RMTCB}RegDef[PageWordE,R,0B];{last Page and Word to be tested}RegDef[DataO,R,0C];   {Observed data at this address - For RMTCB}RegDef[DR,R,0D];	 {Data mask Reg, clobbered in send/rcv Mctb}RegDef[TrapState,RH,0D]; {In case of trap, what were we doing}RegDef[Interface, R, 0E]; {IOP/MP interface control}RegDef[RF,R,0F];	 {clobberd in send/rcv Mctb}RegDef[rhRF,RH,0F];   {}RegDef[Passes, U, 10];      {From SMTCB Word 1, # of passes to run}RegDef[BankS, U, 11];       {From SMTCB Word 2, starting Bank}RegDef[BankE, U, 14];       {From SMTCB Word 3, ending Bank}RegDef[MPageWordS, U,17];       {From SMTCB Word 5, Map Page start}RegDef[MBankS, U,18];       {From SMTCB Word 5, Map Bank start}RegDef[MPageBankE, U,19];       {From SMTCB Word 5, Map Page end}RegDef[DataS, U, 1B];       {From SMTCB Word 7, starting Data pattern}RegDef[DataT, U, 1C];       {From SMTCB Word 8, starting Data Type}RegDef[TstCtrl, U, 1D];     {From SMTCB Word 8, Test Control - test to be run}RegDef[DatErrCtl, U, 1E];   {From SMTCB Word 9, Data and Error Control}RegDef[Delay, U, 1F];       {From SMTCB Word 10,time between Store and Check}RegDef[MemorySize, U, 20];  {For RMTCB Word 1, last size of Memory found by test}RegDef[LDataSP, U, 21];	    {Log register for Data Start patt. this Pass}RegDef[LDataTP, U, 22];     {Log reg for Data Type this Pass}RegDef[LDataO, U, 23];      {Log reg for ReadData}RegDef[LDataE, U, 24];      {Log reg for Expect}RegDef[Fault, U, 25];      {Log reg for Faults}RegDef[LBankPage, U, 26];      {Log reg for BankPage}RegDef[LWord, U, 27];      {Log reg for Word}RegDef[MemorySize1, U, 30];  {}Set[SMTCBp1,   8E];  {Recognition}Set[SMTCBp2,  0E8];   {pattern - 8EE8}Set[RMTCBp1,   4C];  {Our recognition pattern}Set[RMTCBp2,  0C4];   {back to IOP - 4CC4}Set[MTCBlocl,  00];  {location we expect SMTCB (low-order 8 bits)}Set[MTCBlocm,0000];   {middle}Set[MTCBloch,  00];   {rh part of S&R MTCB location}