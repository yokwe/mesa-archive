¬ FALSE,        -- for configure command    needConfigureParams(1:4..4): BOOLEAN ¬ FALSE,    -- spares    spare5(1:5..5): BOOLEAN ¬ FALSE,    spare6(1:6..6): BOOLEAN ¬ FALSE,    spare7(1:7..7): BOOLEAN ¬ FALSE,    spare8(1:8..8): BOOLEAN ¬ FALSE,    spare9(1:9..9): BOOLEAN ¬ FALSE,    spare10(1:10..10): BOOLEAN ¬ FALSE,    spare11(1:11..11): BOOLEAN ¬ FALSE,    spare12(1:12..12): BOOLEAN ¬ FALSE,    spare13(1:13..13): BOOLEAN ¬ FALSE,    spare14(1:14..14): BOOLEAN ¬ FALSE,        checkIOStatus(1:15..15): BOOLEAN ¬ FALSE,    -- Names, Explanations and PROCs    commandName(2):	LONG STRING ¬ NIL,    explanation(4): 	LONG STRING ¬ NIL,    cmdProc(6): 	PROC ¬ NIL    ];  END...Log	Time		By	Action19-Dec-84 15:46:40	MXT	Creation13-Feb-85 11:53:27	MXT	Added BOOLEANs for the 82586 commands.25-Feb-85 14:48:40	MXT	Moved CmdRecord from DiagEtherCommCmdDes.mesa. 7-Mar-85 17:27:30	MXT	Added cmds and Commands. 8-Jul-85 16:32:45	MXT	Added checkIOStatus