-- XCharSet46.mesa  Greek Alphabet-- Last edited by:-- JGS on	21-Nov-83 11:14:47DIRECTORY  XChar USING [Character, CharRep],  XCharSets USING [Sets];XCharSet46: DEFINITIONS SHARES XChar =  BEGIN  Make: PROC [code: Codes46] RETURNS [XChar.Character] = INLINE {    RETURN[LOOPHOLE[      XChar.CharRep[set: XCharSets.Sets.greek.ORD, code: code.ORD]]]};    -- makes a character for this character set.      Codes46: TYPE = MACHINE DEPENDENT {    smootheBreathing(45B), roughBreathing(46B), iotaScript(47B),    upperPrime(64B), lowerPrime(65B),    raisedPeriod(73B),    upperAlpha(101B), upperBeta(102B),    upperGamma(104B), upperDelta(105B), upperEpsilon(106B), upperStigma(107B),    upperDigamma(110B), upperZeta(111B), upperEta(112B), upperTheta(113B),    upperIota(114B), upperKappa(115B), upperLambda(116B), upperMu(117B),    upperNu(120B), upperXi(121B), upperOmicron(122B), upperPi(123B),    upperKoppa(124B), upperRho(125B), upperSigma(126B), a127B(127B),    upperTau(130B), upperUpsilon(131B), upperPhi(132B), upperKhi(133B),    upperPsi(134B), upperOmega(135B), upperSampi(136B),    lowerAlpha(141B), lowerBeta(142B), lowerBetaMiddleWord(143B),    lowerGamma(144B), lowerDelta(145B), lowerEpsilon(146B), lowerStigma(147B),    lowerDigamma(150B), lowerZeta(151B), lowerEta(152B), lowerTheta(153B),    lowerIota(154B), lowerKappa(155B), lowerLambda(156B), lowerMu(157B),    lowerNu(160B), lowerXi(161B), lowerOmicron(162B), lowerPi(163B),    lowerKoppa(164B), lowerRho(165B), lowerSigma(166B), lowerSigmaMiddleWord(167B),    lowerTau(170B), lowerUpsilon(171B), lowerPhi(172B), lowerKhi(173B),    lowerPsi(174B), lowerOmega(175B), lowerSampi(176B),    escape(377B)};          END. -- of XCharSet46LOG	( date - person - action )15-Aug-83 11:52:10 - JGS - Created.