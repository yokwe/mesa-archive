--  NSSessionPrivate.Mesa--  Last revised by Saund:   24-Oct-83  9:55:09--  Overview:  Internal session related interfaces.--DIRECTORY   NSSession USING [     Session, Context, ContextID, ClientID, Attributes,     TerminationHandler, Restrictions];NSSessionPrivate: DEFINITIONS =   BEGIN   ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ----  ----   -- TYPES   ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ----  ----   SessionHandleVectorObject: TYPE = RECORD [      currentSessionHandleCount: CARDINAL _ 0,        maxSessionHandleCount: CARDINAL _ 0,        sessionHandleVectorDescriptor: SessionHandleVectorDescriptor _ NIL        ];   maxSessions: CARDINAL = 8;     sizeSessionVector: CARDINAL = 16;     SessionHandleVectorDescriptor: TYPE = LONG DESCRIPTOR FOR ARRAY [1..1] OF      SessionInternal;    SessionHandleVector: TYPE = RECORD [      SEQUENCE COMPUTED CARDINAL OF SessionInternal];   SessionInternal: TYPE = LONG POINTER TO SessionObject;   SessionObject: TYPE = RECORD [      clientID: NSSession.ClientID,      stamp: CARDINAL _ 0, -- time stamp for validation      sessionInUse: BOOLEAN _ FALSE,      sessionAttributes: NSSession.Attributes,       enterSessionCount: CARDINAL _ 0,        lockClientIDRefCount: CARDINAL _ 0,      contextList: ContextList        ];      ContextInternal: TYPE = LONG POINTER TO ContextObject;   ContextList: TYPE = LONG POINTER TO ContextObject;   ContextObject: TYPE = RECORD [      contextID: NSSession.ContextID,      serviceDataInitialized: BOOLEAN,        context: NSSession.Context,        terminationHandler: NSSession.TerminationHandler,        next: ContextList        ];         RestrictList: TYPE = LONG POINTER TO RestrictObject;   RestrictObject: TYPE = RECORD[     clientID: NSSession.ClientID,     restrictions: NSSession.Restrictions,     totalSessions: CARDINAL _ 0,     next: RestrictList _ NIL     ];         ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ----  ----   -- SIGNALS AND ERRORS   ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ----  ----   InternalError: ERROR [internalErrorType: InternalErrorType];   InternalErrorType: TYPE = {      inconsistentCountsInAllocateSessionHandle,       incorrectRefCount};   ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ----  ----   -- CONSTANTS   ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ----  ----   -- None currently defined.   ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ----  ----   -- PROCEDURES   ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ---- ----  ----   -- Miscellaneous:      SessionIsLocal: PROCEDURE [session: NSSession.Session] RETURNS [BOOLEAN];      END.  -- of NSSessionPrivate LOG  [Date - Person - Action]24-Oct-83  9:54:49 - Saund - REDESIGN