-- NSStringPriv.mesa-- last revised by Liu   3-Sep-82 14:59:30-- private stuff used by NSString(Test)ImplDIRECTORY  Environment USING [Byte],  NSString;  NSStringPriv:  DEFINITIONS =BEGIN OPEN NSString;AccentRange: TYPE = Environment.Byte [300B..317B];Bytes: TYPE = RECORD [PACKED SEQUENCE COMPUTED CARDINAL OF Environment.Byte];BytesPointer: TYPE = LONG POINTER TO Bytes;Context: TYPE = POINTER TO ContextRecord; ContextRecord: TYPE = RECORD [  string: POINTER TO String, chset: CARDINAL, position: CARDINAL];  --  Position within an NSString.  -- PROCS-- Return TRUE if we're at the end of the string.AtEnd: PROC [context: Context] RETURNS [BOOLEAN] = INLINE    { RETURN[context.position >= context.string.length] };    -- Adjust the context so that it points to the next NS character (not the next byte).  Assumes a valid string.BumpContext: PROC [context: Context, ignoreAccents: BOOLEAN _ TRUE];InitContextBegin: PROC [context: Context, ignoreAccents: BOOLEAN] RETURNS [ctAccents: CARDINAL];InitContextEnd: PROC [context: Context, string: POINTER TO String];InitContextFromSubString: PROC [context: Context, substring: SubString, ignoreAccents: BOOLEAN];    END.LOG [Date - person - action]15-Mar-82 17:33:44 - Hamilton - Created from NSStringImpl. 3-May-82 11:47:24 - Hamilton - Fix InitContextBegin by adding ignoreAccents BOOLEAN. 1-Aug-82 17:17:12 - Hamilton - removed "string" argument from InitContextBegin (since SubString is now a pointer).  Add ignoreAccents BOOLEAN to InitContextFromSubString and ctAccents CARDINAL returned from InitContextBegin. 3-Sep-82 - Liu - removed CharacterI, replaced by NSString.Character.