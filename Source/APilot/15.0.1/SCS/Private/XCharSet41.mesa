-- XCharSet41.mesa  JIS Symbols 1  -  Punctuation & Symbols not in Set 0-- Last edited by:-- JGS on	28-Nov-83 14:49:48DIRECTORY  XChar USING [Character, CharRep],  XCharSets USING [Sets];XCharSet41: DEFINITIONS SHARES XChar =  BEGIN  Make: PROC [code: Codes41] RETURNS [XChar.Character] = INLINE {    RETURN[LOOPHOLE[      XChar.CharRep[set: XCharSets.Sets.jisSymbol1.ORD, code: code.ORD]]]};    -- makes a character for this character set.      Codes41: TYPE = MACHINE DEPENDENT {    kanjiSpace(41B),        japaneseComma(42B), japanesePeriod(43B),    dakuonMark(53B),    handakuonMark(54B),    repeatHiragana(63B), repeatHiraganaWithDakuon(64B),    repeatKatakana(65B), repeatKatakanaWithDakuon(66B),        reduplicate(67B),    reduplicateAboveItem(70B), repeatKanji(71B), shime(72B), kanjiZero(73B),    longVowelBar(74B), hyphen(76B),    parallelSign(102B), threeDotLeader(104B), twoDotLeader(105B),    leftBrokenBracket(114B), rightBrokenBracket(115B),    leftJapaneseQuote(126B), rightJapaneseQuote(127B),    leftJapaneseDoubleQuote(130B), rightJapaneseDoubleQuote(131B),    leftBlackLenticularBracket(132B), rightBlackLenticularBracket(133B),    notEqual(142B), lessThanOrEqualTo(145B), greaterThanOrEqualTo(146B),       infinity(147B),    therefore(150B), male(151B), female(152B),    minutes(154B), seconds(155B), degreesCelsius(156B),    whiteStar(171B), blackStar(172B), whiteCircle(173B),    blackCircle(174B), bullsEye(175B), whiteDiamond(176B),    escape(377B)};          END. -- of XCharSet41LOG	( date - person - action )15-Aug-83 11:52:10 - JGS - Created.