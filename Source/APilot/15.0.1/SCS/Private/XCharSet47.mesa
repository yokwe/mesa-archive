-- XCharSet47.mesa  Cyrillic Alphabet-- Last edited by:-- JGS on	28-Nov-83 14:51:20DIRECTORY  XChar USING [Character, CharRep],  XCharSets USING [Sets];XCharSet47: DEFINITIONS SHARES XChar =  BEGIN  Make: PROC [code: Codes47] RETURNS [XChar.Character] = INLINE {    RETURN[LOOPHOLE[      XChar.CharRep[set: XCharSets.Sets.cyrillic.ORD, code: code.ORD]]]};    -- makes a character for this character set.      Codes47: TYPE = MACHINE DEPENDENT {    upperA(41B), upperBe(42B), upperVe(43B), upperGe(44B), upperDe(45B),    upperYe(46B), upperYo(47B), upperZhe(50B), upperZe(51B), upperI(52B),    upperIKratkoye(53B), upperKa(54B), upperEl(55B), upperEm(56B),    upperEn(57B), upperO(60B), upperPe(61B), upperEr(62B), upperEs(63B),    upperTe(64B), upperU(65B), upperEf(66B), upperXa(67B), upperTse(70B),    upperChe(71B), upperSha(72B), upperShCha(73B), upperTvyordiiZnak(74B),    upperYeri(75B), upperMyaxkiiZnak(76B), upperEOborotnoye(77B),    upperYu(100B), upperYa(101B),    lowerA(121B), lowerBe(122B), lowerVe(123B), lowerGe(124B), lowerDe(125B),    lowerYe(126B), lowerYo(127B), lowerZhe(130B), lowerZe(131B), lowerI(132B),    lowerIKratkoye(133B), lowerKa(134B), lowerEl(135B), lowerEm(136B),    lowerEn(137B), lowerO(140B), lowerPe(141B), lowerEr(142B), lowerEs(143B),    lowerTe(144B), lowerU(145B), lowerEf(146B), lowerXa(147B), lowerTse(150B),    lowerChe(151B), lowerSha(152B), lowerShCha(153B), lowerTvyordiiZnak(154B),    lowerYeri(155B), lowerMyaxkiiZnak(156B), lowerEOborotnoye(157B),    lowerYu(160B), lowerYa(161B),    escape(377B)};          END. -- of XCharSet47LOG	( date - person - action )15-Aug-83 11:52:10 - JGS - Created.