tput, Inline, headStartChain2];  [headStartChain3] ¬    GermDoveStart[      BootChannelDisk, BootChannelSPP, BootChannelFloppy, GermOps,      GermOpsImplDove, GermOpsLinkageImpl, HeadStartChainPlug,      headStartChainPlug, SimpleNSIOEthernetImpl, TeledebugImpl];  GermMarkersImpl;  [bootChannelPlug, GermOps, ResidentHeap, ResidentMemory, Runtime, System] ¬    GermOpsImpl[      bootChannel0, ControlModuleFormat, Environment, EthernetFace, GermMarkers,      GermOps, Frame, FrameExtras, headStartChain0, Inline, ProcessorFace,      PageMap, PilotDiskFace, ProcessOperations, SpecialRuntimeExtras, Trap];  GermOpsImplDove;  GermOpsLinkageImpl;  headStartChainPlug ¬  HeadStartChainPlug[];  ProcessorHeadDove; -- NOT in HeadStartChain  SimpleNSIOEthernetImpl;  [FloppyCommissure, FloppyDiagnosticFaceDove,FloppyDiskFace,   headStartChain2, SpecialFloppyTapeFace] ¬    FloppyHeadDoveA[      <<DeviceCleanup,>> DoveInputOutput, FloppyCommissure, headStartChain3, Inline];  [headStartChain0, PilotDiskFace, PilotDiskFaceStatistics] ¬    DiskHeadDuke[      CompatibilityDiskFace, CompatibilityDiskFaceExtra,      headStartChain1, Inline];  DiskHeadLabeledDukeA;  DiskHeadLabeledDukeB;  -- EXPORTs CompatibilityDiskFace, CompatibilityDiskFaceExtra,  -- SpecialDiskFaceDove and SpecialPilotDiskFace  TeledebugImpl;  -- (omitted in GermDiskDuke.config)    FloppyHeadDoveB;  END...10-Aug-88  9:23:44  KT   Created from UnpackedGermDove.config12-Oct-88 10:55:51  MN   divided DiskHeadLabeledDuke into DiskHeadLabeledDukeA and DiskHeadLabeledDukeB20-Jan-89 21:57:36  MS   changed FloppyHeadDoveA, FloppyHeadDoveB  for Floppy 1024 support