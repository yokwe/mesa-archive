annelDisk, BootChannelFloppy, GermOps, GermOpsImplDove,      GermOpsLinkageImpl, HeadStartChainPlug, headStartChainPlug];  GermMarkersImpl;  [bootChannelPlug, GermOps, ResidentHeap, ResidentMemory, Runtime, System] ¬    GermOpsImpl[      bootChannel0, ControlModuleFormat, Environment, EthernetFace, GermMarkers,      GermOps, Frame, FrameExtras, headStartChain0, Inline, ProcessorFace,      PageMap, PilotDiskFace, ProcessOperations, SpecialRuntimeExtras, Trap];  GermOpsImplDove;  GermOpsLinkageImpl;  headStartChainPlug ¬  HeadStartChainPlug[];  ProcessorHeadDove; -- NOT in HeadStartChain  [FloppyCommissure, FloppyDiagnosticFaceDove,FloppyDiskFace, headStartChain1,      SpecialFloppyTapeFace] ¬    FloppyHeadDoveA[      <<DeviceCleanup,>> DoveInputOutput, FloppyCommissure, headStartChain2, Inline];  [headStartChain0, PilotDiskFace, PilotDiskFaceStatistics] ¬    DiskHeadDuke[      CompatibilityDiskFace, CompatibilityDiskFaceExtra,      headStartChain1, Inline];  DiskHeadLabeledDukeA;  DiskHeadLabeledDukeB;  -- EXPORTs CompatibilityDiskFace, CompatibilityDiskFaceExtra,  -- SpecialDiskFaceDove and SpecialPilotDiskFace  FloppyHeadDoveB;END...10-Aug-88  9:29:27  KT   Created from UnpackedGermDiskDove.config12-Oct-88 10:49:01  MN   divided DiskHeadLabeledDuke into DiskHeadLabeledDukeA and DiskHeadLabeledDukeB20-Jan-89 22:00:57  MS   changed FloppyHeadDoveA, FloppyHeadDoveB  for Floppy 1024 support  