-- RS232CIOHeadsDove.pack-- BKI    13-Jan-88 15:35:24 Support for new NewRS232CFace-- RRP    20-Aug-87 14:16:29 Moved InitializeCleanup, added WorkaroundForAR9282-- Created by DeS    on:   18-Jun-85 20:44:20-- Copyright (C) 1987, 1988 by Xerox Corporation. All rights reserved.RS232CIOHeadsDove: SEGMENT =BEGINVeryHotRS232CIOHeadsDove: CODE PACK =   BEGIN   RS232CHeadDove  [ENTRY VECTOR, InitializeCleanup, WorkaroundForAR9282,     PollReceiveOrTransmit];   RS232CSubDriver[ENTRY VECTOR, NakedWakeUpHere, SendCommand, SetParameters];    END;HotRS232CIOHeadsDove: CODE PACK =   BEGIN   RS232CHeadDove[DequeueIOCB, EnQueue, InitiateReceive, InitiateTransmit, PollCommand, PollSetParameters];   RS232CSubDriver[LocalGet, LocalPut, LocalTransferWait];    END;WarmRS232CIOHeadsDove: CODE PACK =   BEGIN   RS232CHeadDove[     GetDeviceStatus, InitiateCommand, InitiateSetControlBits,     InitiateSetParameters, PollSetControlBits];   RS232CSubDriver[LocalGetStatus, LocalStatusWait, SetControlBits];   RS366HeadDove[ENTRY VECTOR, GetStatus];   END;CoolRS232CIOHeadsDove: CODE PACK =   BEGIN   RS232CHeadDove[InitiateResetStatusBits, PollResetStatusBits];   RS232CSubDriver[     CATCH CODE, LocalResetLine, LocalSetParameters, LocalSendBreak,     ResetStatusBits];    RS366HeadDove[SetStatus];   END;ColdRS232CIOHeadsDove: CODE PACK =   BEGIN   RS232CHeadDove[AbortReceive, AbortTransmit, GetHandle, GetNextLine, Initialize,       MAIN, ReleaseHandle, RS232CHeadInit, CATCH CODE];   RS232CSubDriver[GetNextLine, Init, LocalAbort, LocalAutoRecognitionWait, LocalOff,      MAIN, On];   RS366HeadDove[MAIN, GetDialerCount];   END;END;-- Frame PacksHotRS232CIOHeadsD0Frames: FRAME PACK =   BEGIN   RS232CSubDriver;   RS232CHeadDove;   RS366HeadDove;   END;