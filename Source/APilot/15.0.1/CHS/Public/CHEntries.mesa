0), Austrian(1), AustrianAlt(2), German(3), GermanAlt(4),    Belgian(5), Brazilian(6), CanadianFrench(7), Danish(8), DanishAlt(9),    Norwegian(10), NorwegianAlt(11), Finnish(12), FinnishAlt(13), Swedish(14),    SwedishAlt(15), French(16), International(17), Italian(18),    JapaneseEnglish(19), JapaneseKana(20), Portuguese(21), Spanish(22),    SpanishAlt(23), SpanishSpeaking(24), UKenglish(25), unused1(26), unused2(27),    unused3(28), unused4(29), unused5(30), unused6(31), (LAST[CARDINAL])};  IBMDeviceType: TYPE = MACHINE DEPENDENT{    unused(0), model1(1), model2(2), model3(3), model4(4), model5(5), printer(6),    other(7), (LAST[CARDINAL])};  -- Siemens9750Host entry  Siemens9750HostData: TYPE = MACHINE DEPENDENT RECORD [    controllers(0): LONG DESCRIPTOR FOR ARRAY OF Siemens9750Controller];  Siemens9750Controller: TYPE = MACHINE DEPENDENT RECORD [    controllerAddress(0): CARDINAL,    portsOnController(1): CARDINAL,    linkType(2): ControllerLinkType,    language(3): SiemensLanguages,    terminalModelType(4): LONG DESCRIPTOR FOR ARRAY OF SiemensTerminalType];  SiemensTerminalType: TYPE = MACHINE DEPENDENT{    unused(0), model1(1), model2(2), model3(3), model4(4), model5(5), (LAST[    CARDINAL])};  SiemensLanguages: TYPE = MACHINE DEPENDENT{    USenglish(0), Austrian(1), AustrianAlt(2), German(3), GermanAlt(4),    Belgian(5), Brazilian(6), CanadianFrench(7), Danish(8), DanishAlt(9),    Norwegian(10), NorwegianAlt(11), Finnish(12), FinnishAlt(13), Swedish(14),    SwedishAlt(15), French(16), International(17), Italian(18),    JapaneseEnglish(19), JapaneseKana(20), Portuguese(21), Spanish(22),    SpanishAlt(23), SpanishSpeaking(24), UKenglish(25), unused1(26), unused2(27),    unused3(28), unused4(29), unused5(30), unused6(31), (LAST[CARDINAL])};  -- RS232C port entry  RS232CData: TYPE = MACHINE DEPENDENT RECORD [    cIUPort(0): BOOLEAN,    owningClientType(1): PortClientType,    preemptionAllowed(2): BOOLEAN,  -- if TRUE, owningClients can be preempted.    lineNumber(3): CARDINAL,  -- logical number    dialerNumber(4): CARDINAL,    -- logical dialer number, must be unique    -- within domain (can have same value as a lineNumber)    -- port/modem hardware characteristics:    duplexity(5): RS232CEnvironment.Duplexity,    dialingHardware(6): PortDialerType,    -- information needed for the configuration of dedicated ports    --   and/or 87x software:    charLength(7): RS232CEnvironment.CharLength,  -- only async port application    echoing(8): PortEchoingLocation,  -- only async port application    flowControl(9): RS232CEnvironment.FlowControl,  -- only async port application    lineSpeed(12): RS232CEnvironment.LineSpeed,    parity(13): RS232CEnvironment.Parity,  -- only async port application    stopBits(14): RS232CEnvironment.StopBits,  -- only async port application    portActsAsDCE(15): BOOLEAN,  -- only async port application    accessControl(16): NSName.Name,  -- group allowed to use this port    validLineSpeeds(18): LONG DESCRIPTOR FOR ARRAY OF      RS232CEnvironment.LineSpeed];  -- valid line speeds for this port  PortClientType: TYPE = MACHINE DEPENDENT{    unassigned, outOfService, its, irs, gws, ibm3270Host, ttyEmulation, rbs, fax,    mailGateway, phototypesetter};  PortDialerType: TYPE = MACHINE DEPENDENT{none, vadic, hayes, ventel, rs366};  PortEchoingLocation: TYPE = MACHINE DEPENDENT{application, ciu, terminal};  PortSyncType: TYPE = MACHINE DEPENDENT{    asynchronous, synchronous,  -- implies both bit synch and byte synch capabilities    bitSynchronous, byteSynchronous, any};  UserData: TYPE = MACHINE DEPENDENT RECORD [    lastNameIndex(0): CARDINAL, fileService(1): NSName.Name];  -- secondary entries required for backward compatibility only  IBM3270HostBack: TYPE = MACHINE DEPENDENT RECORD [path(0): NSName.Name];  -- name of rs232c port  RS232CBack: TYPE = MACHINE DEPENDENT RECORD [    owningCIU(0): NSString.String,  -- name of CIU if port on a CIU    owningECS(4): NSString.String,  -- name of ECS that "owns" this port    owningClient(8): NSString.String,  -- name of client using the port    portNumber(12): CARDINAL];  -- physical line number  -- generic structures  AddressList: TYPE = LONG DESCRIPTOR FOR ARRAY OF System.NetworkAddress;  END..   LOG 1-Nov-83 14:58:31 - read - created 11-Nov-83 18:31:22 - read 11-Nov-83 16:18:26 - brd - Defined IBM 3270, Siemens 9750 and RS232C entries 2-Jan-84 23:02:40 - Gealy - Added to PortClientType and PortDialerType.