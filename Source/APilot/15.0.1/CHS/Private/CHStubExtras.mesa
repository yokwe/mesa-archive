 RETURNS [CARDINAL];  CardinalsToCourierProcedures: PROCEDURE [c: CARDINAL]    RETURNS [p: CHProtocol.CourierProcedures];  ProcsUseParms: PROCEDURE [p: CHProtocol.CourierProcedures]    RETURNS [parms: CHProtocol.Parms];  ProcsUseResult: PROCEDURE [p: CHProtocol.CourierProcedures]    RETURNS [results: CHProtocol.Results];  MakeCallParameter: PUBLIC PROCEDURE [parm: CHMUX.Parm, sending: BOOLEAN]    RETURNS [Courier.Parameters];  MakeReturnParameter: PUBLIC PROCEDURE [    result: CHMUX.Result, sending: BOOLEAN, version: CHProtocol.Version]    RETURNS [Courier.Parameters];  GetCourierInfo: PROCEDURE [    chProcedure: CHProtocol.CourierProcedures, version: CHProtocol.Version]    RETURNS [      programNumber: LONG CARDINAL, versionNumber: CARDINAL,      procedureNumber: CARDINAL];  END.  LOG25-Feb-85 16:45:35 - Mweeks - created  