];  [headStartChain0, PilotDiskFace] ¬    DiskHeadGuam[      DeviceCleanup, GuamInputOutput, headStartChain1];  [EthernetFace, headStartChain1] ¬    EthernetHeadGuam[       DeviceCleanup, GuamInputOutput, headStartChain2, Inline];  [FloppyDiskFace, headStartChain2, SpecialFloppyTapeFace] ¬    FloppyHeadGuam[       DeviceCleanup, GuamInputOutput, headStartChain3];  [headStartChain4] ¬    GermGuamStart[      BootChannelDisk, BootChannelSPP, BootChannelStream, BootChannelFloppy,      GermOps, GermOpsImplGuam, GermOpsLinkageImpl, HeadStartChainPlug,      headStartChainPlug, SimpleNSIOEthernetImpl, TeledebugImpl];  GermMarkersImpl;  [bootChannelPlug, GermOps, ResidentHeap, ResidentMemory, Runtime, System] ¬    GermOpsImpl[      bootChannel0, ControlModuleFormat, Environment, EthernetFace, GermMarkers,      GermOps, Frame, FrameExtras, headStartChain0, Inline, ProcessorFace,      PageMap, PilotDiskFace, ProcessOperations, SpecialRuntimeExtras, Trap];  GermOpsImplGuam;  GermOpsLinkageImpl;  headStartChainPlug ¬  HeadStartChainPlug[];  ProcessorHeadGuam; -- NOT in HeadStartChain  SimpleNSIOEthernetImpl;  TeledebugImpl;    END... 8-Mar-93	JPM	Created from UnpackedGermBounty.config26-Mar-93	JPM	Added Inline to EthernetHeadGuam imports list 7-Jul-93	JPM	Added BootChannelStream and CoProcessorHeadGuam