nup, Environment, headStartChain2,        ProcessOperations];  [FloppyDiskFace, headStartChain2, SpecialFloppyTapeFace] ¬    FloppyHeadBounty[      BountyInputOutput, DeviceCleanup, headStartChain3, ProcessOperations];  [headStartChain3] ¬    GermBountyStart[      BootChannelDisk, BootChannelSPP, BootChannelFloppy, GermOps,      GermOpsImplBounty, GermOpsLinkageImpl, HeadStartChainPlug,      headStartChainPlug, SimpleNSIOEthernetImpl, TeledebugImpl];  GermMarkersImpl;  [bootChannelPlug, GermOps, ResidentHeap, ResidentMemory, Runtime, System] ¬    GermOpsImpl[      bootChannel0, ControlModuleFormat, Environment, EthernetFace, GermMarkers,      GermOps, Frame, FrameExtras, headStartChain0, Inline, ProcessorFace,      PageMap, PilotDiskFace, ProcessOperations, SpecialRuntimeExtras, Trap];  GermOpsImplBounty;  GermOpsLinkageImpl;  headStartChainPlug ¬  HeadStartChainPlug[];  ProcessorHeadBounty; -- NOT in HeadStartChain  SimpleNSIOEthernetImpl;  TeledebugImpl;    END... 4-Oct-89	JPM	Created from UnpackedGermDuke.config 25-Feb-92 10:48:58	CSalmond	fixed binding errors.