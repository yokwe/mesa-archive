opbackHelp,      resetHelp,   calibrateHelp,   intDiagHelp,   getOptHelp,   getFaultsHelp,      numOfRepeatHelp,   stopAndAskHelp0,   stopAndAskHelp1,   stopAndAskHelp2,      scnrVersionHelp,   noChoiceHelp,   noChoiceHelp1,      -- misc messages and prompt   numOfRepeat,   stopAndAsk,   cantContinue,   promptForCont,   errsEncountered,   faultsList,   abortACK,   initError,      repeatInfo,   desiredRepeatCount,   repetitionsCompleted,   errStopSoFar,   returnControl,   done,   Yes,   No,   areYouSure,   aborted,   listIsEmpty,   runBasicScnrFirst,   initializing,   unrecoverableErr,   -- Inca Driver generated fault codes   unIDBoard,   multiBoard,   f0,   f1,   f2,   f3,   f4,   f5,   f6,   f7,   f8,   fb,   fc,      scnrVersion,   twilightZone  };    -- Error list messages  scnrErrorMessage: READONLY POINTER TO ARRAY ScnrErrorMessages OF LONG STRING;  ScnrErrorMessages: TYPE = IncaDiagDefs.ErrorType;  <<  ScnrErrorMessages: TYPE = { -- Table of enumerated indices for error messages.     driverNotReady,   insufSpace,   basicLoopCompare,   filterLoopCompare,		    --Communication errors   comlink,   sndTimeout,   recTimeout,   comerror,      --Special Timeout Errors   noVCICommun,   hungprescan,   hungbasicLoop,   hungFilterLoop,   hungReset,      --Port Errors   noHardware,   noSoftware,       --Port to Scanner Errors   connectUnknown,   notConnect,   notPowered,   resetFault,      --Diagnostic Found Board Problem in   rs485comError,   videoContr,   videoProcess,   motorDriver,   filterBoard,   compressBoard,   lampBad,   --Other Scanner Internal Errors   reqReset,   opcodeReject,   dataReject,   stateReject,   hardwareRej,   noScanHdwRej   };   >>          END... of ScannerMsgKeysDove.mesa  LOG21-Jan-87 15:20:23 JMA	- Creation29-Jun-87 18:04:21 JMA  - updated to match IncaDiagDefs