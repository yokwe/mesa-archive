data tests	STwrTrk25With1s,	STrdVrTrk25WithConstDt,	STwrTrk50With0s,	STrdVrTrk50WithConstDt,	STwrTrk75WithAs,	STrdVrTrk75WithConstDt,	STwrTrk100With6s,	STrdVrTrk100WithConstDt,	STwrDiaDtAcrossTrks,	STrdVrDiaDtAcrossTrks,	STwrDiaDtAcrossStrm,	STrdVrDiaDtAcrossStrm,	-- track address test	STwr12TrksEvnStrms,	STwr12TrksOddStrms,	STrd12TrksOddStrms,	STkillTrk2,		STrd12TrksEvnStrms,	STkillTrk245,	-- write/read/verify test	STwrKnwnDtStrm6Ex,	STrdVrKnwnDtStrm6Ex,	STwrRdVr4TrksStrm7,		--CONFIDENCE SUBTESTS	-- sector address test (STrdVrTrk0EachSec from exerciser)	STisDiagTape, --Note: needs bad page table	-- read streams 6&7 tests	STrdVrStrm6A7Tst1,	STrdVrStrm6A7Tst2,	--check if drive accepts all stream pulses before writing on tape	STrdIDStrm12,--	STformatStrm4,--	STformatStrm5,	--check if the format happened	STrdVrTrk735WithAll1s,	--do stream address testing	STrdIDsEvnStrms2to8,	STrdIDOdd1To7Strms,	--data pattern tests	STwrStrtTrk735With0s,	STrdVrTrkStrt735With0s,	STwrStrtTrk759With1s,	STrdVrTrkStrt759With1s,	STwrStrtTrk783With5s,	STrdVrTrkStrt783With5s,	STwrStrtTrk807WithAs,	STrdVrTrkStrt807WithAs,	STwrStrtTrk831With9249,	STrdVrTrkStrt831With9249,	STwrStrtTrk855With2492,	STrdVrTrkStrt855With2492,	STwrStrtTrk879With4924,	STrdVrTrkStrt879With4924,	STwrStrtTrk903With6DB6,	STrdVrTrkStrt903With6DB6,	STwrStrtTrk927WithDB6D,	STrdVrTrkStrt927WithDB6D,	STwrStrtTrk951WithB6DB,	STrdVrTrkStrt951WithB6DB,	STwrStrtTrk975With1111,	STrdVrTrkStrt975With1111,	STwrStrtTrk999With2222,	STrdVrTrkStrt999With2222,	STwrStrtTrk1023With4444,	STrdVrTrkStrt1023With4444,	STwrStrtTrk1047With8888,	STrdVrTrkStrt1047With8888,	STwrStrtTrk1071With7777,	STrdVrTrkStrt1071With7777,	STwrStrtTrk1095WithEEEE,	STrdVrTrkStrt1095WithEEEE,	STwrStrtTrk1119WithDDDD,	STrdVrTrkStrt1119WithDDDD,	STwrStrtTrk1143WithBBBB,	STrdVrTrkStrt1143WithBBBB,	STwrStrtTrk1167WithDiagDt,	STrdVrTrkStrt1167WithDiagDt,	STwrTrk735WithDiagDt,	STrdVrTrk735WithDiagDt,		-- SURFACE VERIFICATION SUBTEST	STsurfVrStrm1,	STsurfVrStrm2,	STsurfVrStrm3,	STsurfVrStrm4,	STsurfVrStrm5,	STsurfVrStrm6,	STsurfVrStrm7,	STsurfVrStrm8,	STsurfVrStrm9,	STsurfVrStrm10,	STsurfVrStrm11,	STsurfVrStrm12,		--MISCELANEOUS	STrdIDStrm1,	STrdIDsEvnStrms,	STrdIDsOddStrms      };	 Subtests:	READONLY LONG POINTER TO ARRAY Subtest OF SubTestRecord;   DiagFlTapeTestRecImpl:	PROGRAM;     -- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -- to run an array of tests -- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~   RunTestArray: PROCEDURE [displayTitle: BOOLEAN¬ TRUE, 	subtestSelectArray: LONG DESCRIPTOR FOR ARRAY OF Subtest];   -- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -- to run a test -- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~   RunSubtest: PROCEDURE [	displayTitle: BOOLEAN¬ TRUE,	subtest: Subtest];       END... -- DiagFlTapeTestRec   LOG