-- CHEntriesExtrasImpl.mesa-- last edited by read,        23-Feb-84 18:36:15DIRECTORY  CHEntriesExtras USING [AuthenticationLevel, CIUBack, Siemens9750Controller, Siemens9750Device, Siemens9750HostData],  Courier USING [Description],  NSName USING [DescribeNameRecord],  NSString USING [DescribeString];CHEntriesExtrasImpl: PROGRAM  IMPORTS NSName, NSString  EXPORTS CHEntriesExtras =  BEGIN    DescribeAuthenticationLevel: PUBLIC Courier.Description =    BEGIN    authLevel: LONG POINTER TO CHEntriesExtras.AuthenticationLevel _      notes.noteSize[SIZE[CHEntriesExtras.AuthenticationLevel]];    END;      -- Siemens 9750 Courier Descriptions      DescribeSiemens9750HostData: PUBLIC Courier.Description =    BEGIN OPEN CHEntriesExtras;    hostData: LONG POINTER TO Siemens9750HostData _       notes.noteSize[SIZE[Siemens9750HostData]];    notes.noteArrayDescriptor[@hostData.controllers, SIZE[Siemens9750Controller],       32];    FOR i: CARDINAL IN [0..LENGTH[hostData.controllers]) DO      notes.noteParameters[@hostData.controllers[i],	 DescribeSiemens9750Controller];      ENDLOOP;    END;    DescribeSiemens9750Controller: Courier.Description =    BEGIN OPEN CHEntriesExtras;    controller: LONG POINTER TO Siemens9750Controller _      notes.noteSize[SIZE[Siemens9750Controller]];    notes.noteArrayDescriptor[@controller.devices, SIZE[Siemens9750Device], 32];    FOR i: CARDINAL IN [0..LENGTH[controller.devices]) DO      notes.noteParameters[@controller.devices[i], DescribeSiemens9750Device];      ENDLOOP;    END;      DescribeSiemens9750Device: Courier.Description =    BEGIN OPEN CHEntriesExtras;    device: LONG POINTER TO Siemens9750Device _      notes.noteSize[SIZE[Siemens9750Device]];    notes.noteDisjointData[@device.accessControl, NSName.DescribeNameRecord];    END;      DescribeCIUBack: PUBLIC Courier.Description =    BEGIN    ciuback: LONG POINTER TO CHEntriesExtras.CIUBack _ notes.noteSize[SIZE[CHEntriesExtras.CIUBack]];    notes.noteParameters[@ciuback.owningECS, NSString.DescribeString];    notes.noteLongCardinal[@LOOPHOLE[ciuback.timestamp, LONG CARDINAL]];    END;      END.  LOG 3-Feb-84 13:26:16, created by read21-Feb-84 15:59:56, read, added DescribeCIUBack23-Feb-84 18:23:56, read, added Siemens stuff for Michalene