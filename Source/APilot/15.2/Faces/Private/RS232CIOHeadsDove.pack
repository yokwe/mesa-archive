-- RS232CIOHeadsDove.pack-- GRG/RSV 21-Sep-90 17:32:30 Took out reference to old routines.-- GRG     23-May-90 16:56:49 Changes for SPAR 219.-- BKI     13-Jan-88 15:35:24 Support for new NewRS232CFace-- RRP     20-Aug-87 14:16:29 Moved InitializeCleanup, added WorkaroundForAR9282-- Created by DeS    on:   18-Jun-85 20:44:20-- Copyright (C) 1987, 1988, 1991 by Xerox Corporation. All rights reserved.RS232CIOHeadsDove: SEGMENT =BEGINVeryHotRS232CIOHeadsDove: CODE PACK =   BEGIN   RS232CHeadDove  [ENTRY VECTOR, InitializeCleanup, WorkaroundForAR9282,     PollReceiveOrTransmit];   RS232CSubDriver[ENTRY VECTOR, NakedWakeUpHere, SendCommand, SetParameters];    END;HotRS232CIOHeadsDove: CODE PACK =   BEGIN   RS232CHeadDove[DequeueIOCB, Enqueue, InitiateReceive,  InitiateTransmit, PollCommand,     PollSetParameters, ReviveInputQueue];   RS232CSubDriver[LocalGet, LocalPut, LocalTransferWait];    END;WarmRS232CIOHeadsDove: CODE PACK =   BEGIN   RS232CHeadDove[     GetDeviceStatus, InitiateCommand, InitiateSetControlBits,     InitiateSetParameters, PollSetControlBits, QueueIsBad];   RS232CSubDriver[     LocalGetStatus, LocalStatusWait, ReviveIfNeeded, SetControlBits];   RS366HeadDove[ENTRY VECTOR, GetStatus];   END;CoolRS232CIOHeadsDove: CODE PACK =   BEGIN   RS232CHeadDove[InitiateResetStatusBits, PollResetStatusBits];   RS232CSubDriver[     CATCH CODE, LocalResetLine, LocalSetParameters, LocalSendBreak,     ResetStatusBits, ReviveQueue];    RS366HeadDove[SetStatus];   END;ColdRS232CIOHeadsDove: CODE PACK =   BEGIN   RS232CHeadDove[AbortReceive, AbortTransmit, GetHandle, GetNextLine, Initialize,       IsReviveProcNeeded, MAIN, ReleaseHandle, RS232CHeadInit, CATCH CODE];   RS232CSubDriver[GetNextLine, Init, LocalAbort, LocalAutoRecognitionWait, LocalOff,      MAIN, On];   RS366HeadDove[MAIN, GetDialerCount];   END;END;-- Frame PacksHotRS232CIOHeadsD0Frames: FRAME PACK =   BEGIN   RS232CSubDriver;   RS232CHeadDove;   RS366HeadDove;   END;