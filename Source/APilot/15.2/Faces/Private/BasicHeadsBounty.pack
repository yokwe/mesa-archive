very seldom  FloppyHeadBounty [    DiskChangeClear, GetContext, GetDeviceAttributes, GetLogStatus, GetNextDevice,    GetStatistics, GetTapeParameters, Initiate, LogicalAddressToPhysicalAddress, MapFix,    MaxTracksPerFormatOperation, Poll, Reset, Retention, SetContext, SuggestedTries];  };SwappableTTYHeads: CODE PACK =  BEGIN  TTYPortHeadBounty[    FcbTxBufferEmpty, GetCommand, GetLineCount, GetStatus, Off, On, PutCommand, SendBreak,    SetParameter];  END;BasicHeadsInitialization: CODE PACK =  BEGIN  ProcessorHeadBounty [GetNextAvailableVM, Start, UnimplementedTrapHandler, MAIN];  EthernetHeadBounty [Start, MAIN];  HeadStartChainPlug [Start, ENTRY VECTOR, MAIN];  DiskHeadBounty [    InitializeController, InitializeTrackBuffer, PagesForTrackBuffering, Start, MAIN];  FloppyHeadBounty [Initialize, Start, MAIN];  SetGMTUsingEthernet [MAIN, EthernetExists, GetNetworkGMT, SetGMT];  TTYPortHeadBounty [Start, MAIN];  UserTerminalHeadBounty [Initialize, Start, MAIN];  RealMemoryImplBounty [    Count, FreeReservedMemory, Initialize, MoveIntervalToDisplayMemory, PCBank, MAIN];  END;END;BasicHeadsResidentFrames: FRAME PACK =  BEGIN  EthernetHeadBounty;  ProcessorHeadBounty;  RealMemoryImplBounty;  DiskHeadBounty;  FloppyHeadBounty;  TTYPortHeadBounty;  SetGMTUsingEthernet;  HeadStartChainPlug;  UserTerminalHeadBounty;  END;