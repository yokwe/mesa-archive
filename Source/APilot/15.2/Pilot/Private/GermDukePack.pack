BeforeMarker;----  END;  Floppy: CODE PACK =  BEGIN  FloppyHeadsDove.Resident;  Floppy.Temporary;  END;----MiddleMarker: CODE PACK =----  BEGIN----  Markers.MiddleMarker;----  END;Initialization: CODE PACK =  BEGIN  BasicHeadsDove.Initialization;  FloppyHeadsDove.Initialization;  DiskHeadsDuke.Initialization;  EthernetHeadsDove.Initialization;  BasicGerm.Initialization;  Floppy.Initialization;  Disk.Initialization;  Ethernet.Initialization;  Start.Initialization;  END;  Discard: DISCARD CODE PACK =  BEGIN  FloppyHeadsDove.Discard;  END;----AfterMarker: CODE PACK =----  BEGIN----  Markers.AfterMarker;----  END;    END; --SEGMENT--    ResidentMergedFrames: FRAME PACK MERGES  BasicHeadsDoveFrames,  FloppyHeadsDoveFrames,  DiskHeadsDukeFrames,  EthernetHeadsDoveFrames,  BasicFrames,  FloppyFrames,  DiskFrames,  EthernetFrames,  StartAndMarkerFrames;-- End of  GermDukePack.pack--    