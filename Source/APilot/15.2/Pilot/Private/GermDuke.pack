dles for size packing.-- 9-Feb-88 17:59:24    Move Initialize and MAIN from Initialization to Resident.-- 20-Jan-89 22:05:11   move InitializeCleanup from FloppyHeadDoveA toFloppyHeadDoveBFloppyHeadsDove: SEGMENT =BEGIN-- memory that can be reclaimed postboot...Initialization: CODE PACK =  BEGIN  FloppyHeadDoveA [Start];  FloppyHeadDoveB [MAIN];  END;Discard: DISCARD CODE PACK =  BEGIN  FloppyHeadDoveA [    DiagnosticGetContext, DiagnosticSuggestedTries,    DiagnosticMaxTracksPerFormatOperation, GetStatistics,    <<InitializeCleanup,>> MaxTracksPerFormatOperation, ResubmitRetention,    SubmitHeldIOCBs];  FloppyHeadDoveB [    DiagnosticAquireLock, DiagnosticDiskChangeClear,    DiagnosticGetDeviceAttributes, DiagnosticGetNextDevice,    DiagnosticInitialize, DiagnosticInitiate, DiagnosticPoll,    DiagnosticReleaseLock, DiagnosticReset, DiagnosticSetContext,    GetTapeParameters, InternalReset, InitializeCleanup, LogicalAddressToPhysicalAddress,    Reset, Retention];  END;Resident: CODE PACK =  BEGIN  FloppyHeadDoveA EXCEPT Initialization, Discard;  FloppyHeadDoveB EXCEPT Initialization, Discard;  END;END; --SEGMENT--FloppyHeadsDoveFrames: FRAME PACK =  BEGIN  FloppyHeadDoveA;  FloppyHeadDoveB;  END;-- End of FloppyHeadsDovePack.pack----Copyright (C) 1984, 1985  by Xerox Corporation. All rights reserved.-- FloppyPack.pack!--30-Oct-84 13:09:30   created file.--20-Mar-85 16:21:12   ENTRY VECTOR from Initialization to Resident.Floppy: SEGMENT =BEGIN-- memory that can be reclaimed postboot...Initialization: CODE PACK =  BEGIN  BootChannelFloppy [MAIN];  END;Resident: CODE PACK =  BEGIN  BootChannelFloppy [ENTRY VECTOR, Create];  END;  -- memory that can be reclaimed postboot...  Temporary: CODE PACK =  BEGIN  BootChannelFloppy EXCEPT Initialization, Resident;  END;    END; --SEGMENT--FloppyFrames: FRAME PACK =  BEGIN  BootChannelFloppy;  END;-- End of FloppyPack.pack--    --Copyright (C) 1984, 1985  by Xerox Corporation. All rights reserved.-- DiskPack.pack!--30-Oct-84 13:09:30   created file.--20-Mar-85 16:21:30   Move ENTRY VECTOR from Initialization to Resident.Disk: SEGMENT =BEGIN-- memory that can be reclaimed postboot...Initialization: CODE PACK =  BEGIN  BootChannelDisk [MAIN];  END;  Resident: CODE PACK =  BEGIN  BootChannelDisk EXCEPT Initialization;  END;    END; --SEGMENT--DiskFrames: FRAME PACK =   BEGIN   BootChannelDisk;   END;   -- End of DiskPack.pack--    -- Copyright (C) 1988 by Fuji Xerox Co., Ltd, Tokyo, Japan.-- All rights reserved. -- DiskHeadsDukePack.pack -- 31-May-88 13:44:03	created file.-- 12-Oct-88 10:09:51	MN--	divided DiskHeadLabeledDuke into DiskHeadLabeledDukeA and DiskHeadLabeledDukeBDiskHeadsDuke: SEGMENT =BEGIN-- memory that can be reclaimed postboot...Initialization: CODE PACK =  BEGIN  DiskHeadLabeledDukeA[    MAIN, GetDeviceType, GetFormattingUnit, Recalibrate];  DiskHeadLabeledDukeB[MAIN, InitializeCleanup];  DiskHeadDuke [MAIN, Start];  END;  Resident: CODE PACK =  BEGIN  DiskHeadLabeledDukeA EXCEPT Initialization;  DiskHeadLabeledDukeB EXCEPT Initialization;  DiskHeadDuke EXCEPT Initialization;  END;    END; --SEGMENT--DiskHeadsDukeFrames: FRAME PACK =  BEGIN  DiskHeadLabeledDukeA;  DiskHeadLabeledDukeB;  DiskHeadDuke;  END;-- End of DiskHeadsDukePack.pack----Copyright (C) 1985, 1987  by Xerox Corporation. All rights reserved.-- EthernetHeadsDovePack.pack!-- 2-Apr-85 16:16:08   Created file.-- 6-Aug-87  8:29:39   Temporary tinkering with packing to reduce size.EthernetHeadsDove: SEGMENT =BEGIN-- memory that can be reclaimed postboot...Initialization: CODE PACK =  BEGIN  EthernetHeadDove [    MAIN, --AddCleanup, --GetPacketsMissed, GetRetries, RemoveCleanup, Start];  END;Resident: CODE PACK =  BEGIN  EthernetHeadDove EXCEPT Initialization;  END;      END; --SEGMENT--EthernetHeadsDoveFrames: FRAME PACK =  BEGIN  EthernetHeadDove;  END;-- End of EthernetHeadsDovePack.pack--    --Copyright (C) 1984, 1985  by Xerox Corporation. All rights reserved.-- EthernetPack.pack!--30-Oct-84 13:09:30   created file.--20-Mar-85 16:22:42   Move ENTRY VECTORs from Initialization to Resident.Ethernet: SEGMENT =BEGIN-- memory that can be reclaimed postboot...Initialization: CODE PACK =  BEGIN  BootChannelSPP [MAIN];  SimpleNSIOEthernetImpl [MAIN];  TeledebugImpl [MAIN];  END;  Resident: CODE PACK =  BEGIN  BootChannelSPP EXCEPT Initialization;  SimpleNSIOEthernetImpl EXCEPT Initialization;  TeledebugImpl EXCEPT Initialization;  END;    END; --SEGMENT--EthernetFrames: FRAME PACK =  BEGIN  BootChannelSPP;  SimpleNSIOEthernetImpl;  TeledebugImpl;  END;-- End of EthernetPack.pack--    -- Copyright (c) 1988 by Fuji Xerox Co., Ltd, Tokyo, Japan. All rights reserved.-- File: GermDukePack.pack last edited by-- 15-Oct-88 13:50:30	MN	changed comments-- 10-Aug-88 10:01:25   KT	Created file.<< The Marker routines are no-op dummies whose sole purpose is to delimit the Floppy code pack and the Initialization code pack.  The order of the code packs is what does the delimiting; don't disturb it! >><< be sure the germ-specific GermDiskDoveStart, GermOpsImplDove are included in this segment >>Start: SEGMENT =BEGIN-- memory that can be reclaimed postboot...Initialization: CODE PACK =  BEGIN  GermDoveStart [MAIN, Start];  GermOpsImplDove [MAIN];  END;  Resident: CODE PACK =  BEGIN  GermDoveStart EXCEPT Initialization;  GermOpsImplDove EXCEPT Initialization;  END;  END; --StartPack SEGMENT----StartFrames: FRAME PACK =--  BEGIN--  GermDoveStart;--  GermOpsImplDove;--  END;Markers: SEGMENT =BEGINGatesOfHellMarker: CODE PACK =  BEGIN  GermMarkersImpl EXCEPT BeforeMarker, MiddleMarker, AfterMarker;   --i.e., [ENTRY VECTOR, MAIN, GatesOfHellMarker]  END;BeforeMarker: CODE PACK =  BEGIN  GermMarkersImpl [BeforeMarker];  END;MiddleMarker: CODE PACK =  BEGIN  GermMarkersImpl [MiddleMarker];  END;AfterMarker: CODE PACK =  BEGIN  GermMarkersImpl [AfterMarker];  END;END; --Markers SEGMENT--StartAndMarkerFrames: FRAME PACK =  BEGIN  GermDoveStart;  GermOpsImplDove;  GermMarkersImpl;  END;  GermDovePack: SEGMENT MERGES   BasicHeadsDove, FloppyHeadsDove, DiskHeadsDuke, EthernetHeadsDove,  BasicGerm, Floppy, Disk, Ethernet, Start, Markers =BEGIN----GatesOfHellMarker: CODE PACK =----  BEGIN----  Markers.GatesOfHellMarker;----  END;Resident: CODE PACK =  BEGIN  BasicHeadsDove.Resident;  DiskHeadsDuke.Resident;  EthernetHeadsDove.Resident;  BasicGerm.Resident;  Floppy.Resident;  Disk.Resident;  Ethernet.Resident;  Start.Resident;  END;----BeforeMarker: CODE PACK =----  BEGIN----  Markers.BeforeMarker;----  END;  Floppy: CODE PACK =  BEGIN  FloppyHeadsDove.Resident;  Floppy.Temporary;  END;----MiddleMarker: CODE PACK =----  BEGIN----  Markers.MiddleMarker;----  END;Initialization: CODE PACK =  BEGIN  BasicHeadsDove.Initialization;  FloppyHeadsDove.Initialization;  DiskHeadsDuke.Initialization;  EthernetHeadsDove.Initialization;  BasicGerm.Initialization;  Floppy.Initialization;  Disk.Initialization;  Ethernet.Initialization;  Start.Initialization;  END;  Discard: DISCARD CODE PACK =  BEGIN  FloppyHeadsDove.Discard;  END;----AfterMarker: CODE PACK =----  BEGIN----  Markers.AfterMarker;----  END;    END; --SEGMENT--    ResidentMergedFrames: FRAME PACK MERGES  BasicHeadsDoveFrames,  FloppyHeadsDoveFrames,  DiskHeadsDukeFrames,  EthernetHeadsDoveFrames,  BasicFrames,  FloppyFrames,  DiskFrames,  EthernetFrames,  StartAndMarkerFrames;-- End of  GermDukePack.pack--    