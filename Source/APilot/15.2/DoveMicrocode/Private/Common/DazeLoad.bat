REM DazeLoad.batREM Copyright (C) 1987 by Xerox Corporation. All rights reserved.BREAK ONREM ----------------------REM Build RAMHInit.obj/RAMLoad.loc!REM ----------------------XFILE RET STKSegs.lnk -oXFILE RET IORSegs.lnk -oXFILE RET Parity.lnk -oXFILE RET TTYHndlr.lnk -oXFILE RET RS232Hdl.lnk -oXFILE RET PCEHndlr.lnk -oXFILE RET RAMOpie.lnk -oXFILE RET MesaUpDn.lnk -oXFILE RET RAMHInit.asm -oXFILE RET IOPDefs.asm -oXFILE RET OpieDefs.asm -oXFILE RET ROMBDefs.asm -oXFILE RET RAMHands.asm -oXFILE RET SegLoc.bat -oCRLFTool /lf RAMHInit.asmDEL RAMHInit.bakCRLFTool /lf IOPDefs.asmDEL IOPDefs.bakCRLFTool /lf OpieDefs.asmDEL OpieDefs.bakCRLFTool /lf ROMBDefs.asmDEL ROMBDefs.bakCRLFTool /lf RAMHands.asmDEL RAMHands.bakCRLFTool /lf SegLoc.batDEL SegLoc.bakRUN ASM86 RAMHInit.asm DEBUG > RAMHInit.logTYPE RAMHInit.logPAUSERUN LINK86 STKSegs.lnk,IORSegs.lnk,RAMHInit.obj,RAMOpie.lnk,Parity.lnk,MesaUpDn.lnk,TTYHndlr.lnk TO Temp.lnkRUN LINK86 Temp.lnk,RS232Hdl.lnk,PCEHndlr.lnk TO RAMLoad.lnkRUN LOC86 RAMLoad.LNK TO RAMLoad.LOC ST(RAMInitialization) &< SegLoc.batREM You should find 2 unresolved external references to ParityHandlerIDREM in RAMLoad.mp2.  These are ok.  This is because the parity handlerREM is linked into RAMLoad in order to reserve space for it.  However,REM the parity entry in RAMHands is commented out so that ParityHandlerIDREM is never generated.  Since the parity handler is never called thisREM reference will never be used.XFILE STORE RAMHInit.obj -oXFILE STORE RAMLoad.lnk -oXFILE STORE RAMLoad.loc -oXFILE STORE RAMLoad.mp2 -oREM ----------------------REM build MemHInit.obj/MemLoad.locREM ----------------------XFILE RET IOPLRAM.obj -oXFILE RET CPBerm.lnk -oXFILE RET SysCnfig.lnk -oXFILE RET PSOHndlr.lnk -oXFILE RET PCEMmHdl.lnk -oXFILE RET MemHInit.asm -oXFILE RET GermInit.asm -oXFILE RET VMMDefs.asm -oXFILE RET HardDefs.asm -oXFILE RET IOPMacro.asm -oXFILE RET CDsplHdl.lnk -oXFILE RET PPOHndlr.lnk -oXFILE RET PSOStub.obj -oCRLFTool /lf HardDefs.asmDEL HardDefs.bakCRLFTool /lf IOPMacro.asmDEL IOPMacro.bakCRLFTool /lf MemHInit.asmDEL MemHInit.bakCRLFTool /lf GermInit.asmDEL GermInit.bakCRLFTool /lf VMMDefs.asmDEL VMMDefs.bakRUN ASM86 MemHInit.asm DEBUG > MemHInit.logRUN ASM86 GermInit.asm DEBUG > GermInit.logTYPE MemHInit.logTYPE GermInit.logPAUSERUN LINK86 IOPLRAM.obj,STKSegs.lnk,IORSegs.lnk,MemHInit.obj,GermInit.obj,SysCnfig.lnk TO Temp.lnkRUN LINK86 Temp.lnk,PSOStub.obj,PPOHndlr.lnk,PCEMmHdl.lnk,CPBerm.lnk,CDsplHdl.lnk TO MemLoad.lnkRUN LOC86 MemLoad.LNK TO MemLoad.LOC &< SegLoc.batXFILE STORE MemHInit.obj -oXFILE STORE GermInit.obj -oXFILE STORE MemLoad.lnk -oXFILE STORE MemLoad.loc -oXFILE STORE MemLoad.mp2 -oREM ----------------------REM  build RAMHIntC.obj/RAMLoadC.locREM ----------------------XFILE RET RAMHndsC.asm -oXFILE RET RAMHIntC.asm -oCRLFTool /lf RAMHndsC.asmDEL RAMHndsC.bakCRLFTool /lf RAMHIntC.asmDEL RAMHIntC.bakRUN ASM86 RAMHIntC.asm DEBUG > RAMHIntC.logTYPE RAMHIntC.logPAUSERUN LINK86 STKSegs.lnk,IORSegs.lnk,RAMHIntC.obj,RAMOpie.lnk,GermInit.obj,MesaUpDn.lnk,TTYHndlr.lnk TO Temp.lnkRUN LINK86 Temp.lnk,SysCnfig.lnk,Parity.lnk,CDsplHdl.lnk,PPOHndlr.lnk TO RAMLoadC.lnkRUN LOC86 RAMLoadC.LNK TO RAMLoadC.LOC ST(RAMInitialization) &< SegLoc.batREM You should find 2 unresolved external references to ParityHandlerIDREM in RAMLoad.mp2.  These are ok.  See the above comment forREM explanation.XFILE STORE RAMHIntC.obj -oXFILE STORE RAMLoadC.lnk -oXFILE STORE RAMLoadC.loc -oXFILE STORE RAMLoadC.mp2 -oREM ----------------------REM  build MemHIntC.obj/MemLoadC.locREM ----------------------XFILE RET MemHIntC.asm -oCRLFTool /lf MemHIntC.asmDEL MemHIntC.bakRUN ASM86 MemHIntC.asm DEBUG > MemHIntC.logTYPE MemHIntC.logPAUSERUN LINK86 IOPLRAM.obj,STKSegs.lnk,IORSegs.lnk,MemHIntC.obj TO MemLoadC.lnkRUN LOC86 MemLoadC.LNK TO MemLoadC.LOC &< SegLoc.batXFILE STORE MemHIntC.obj -oXFILE STORE MemLoadC.lnk -oXFILE STORE MemLoadC.loc -oXFILE STORE MemLoadC.mp2 -oREM ----------------------REM build Debugging symbolsREM ----------------------DEL temp*.*DEL *.logXFILE RET RAMDebug.bat -oCRLFTool /lf RAMDebug.batDEL RAMDebug.bakRAMDebug.batREM RAMDebug.bat AND MesaDove.cm depend on components rebuilt in here!REM fini