 rs232cMpBase + 89;  dialupMpBase:              CARDINAL = 4527;  dialupMpTestStart:         CARDINAL = dialupMpBase;  dialupMpDialing:           CARDINAL = dialupMpBase + 1;  dialupMpCanceling:         CARDINAL = dialupMpBase + 2;  dialupMpDialingTimeout:    CARDINAL = dialupMpBase + 3;  dialupMpTransferTimeout:   CARDINAL = dialupMpBase + 4;  dialupMpOtherFailure:      CARDINAL = dialupMpBase + 5;  dialupMpSuccess:           CARDINAL = dialupMpBase + 6;  dialupMpFailure:           CARDINAL = dialupMpBase + 7;  dialupMpAborted:           CARDINAL = dialupMpBase + 8;  dialupMpFormatError:       CARDINAL = dialupMpBase + 9;  dialupMpTransmissionError: CARDINAL = dialupMpBase + 10;  dialupMpLineOccupied:      CARDINAL = dialupMpBase + 11;  dialupMpNotPresent:        CARDINAL = dialupMpBase + 12;  -- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  --MPCode number definitions for the Lear-Siegler Diagnostics, spanning 2000-2199  -- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  AdjustBase:  CARDINAL = 2080;  --starting MPCode for Adjustment Proc  LSTestBase:  CARDINAL = 2000;  --starting MPCode for Test Proc's  StartAdjust: CARDINAL = AdjustBase + 0;  AdjustOne:   CARDINAL = AdjustBase + 1;  AdjustTwo:   CARDINAL = AdjustBase + 2;  AdjustThree: CARDINAL = AdjustBase + 3;  StartLSTest: CARDINAL = LSTestBase;  END........