1:3..3):		BOOLEAN¬ FALSE,	needSectorCnt(1:4..4):		BOOLEAN¬ FALSE,	-- more stuff needed by IO commands	needData(1:5..5):		BOOLEAN¬ FALSE,	needFillByte(1:6..6):		BOOLEAN¬ FALSE,	needTryCount(1:7..7):		BOOLEAN¬ FALSE,	needUse1Buff(1:8..8):		BOOLEAN¬ FALSE,	spareW1B9(1:9..9):		BOOLEAN¬ FALSE,	needVrfyHdr(1:10..10):		BOOLEAN¬ FALSE,		-- stuff used for error checking	needVrfyData(1:11..11):		BOOLEAN¬ FALSE, --used by error check	need0sInRdBuff(1:12..12):	BOOLEAN¬ FALSE, --used by cmd drive	spareW1B13(1:13..13):		BOOLEAN¬ FALSE,	spareW1B14(1:14..14):		BOOLEAN¬ FALSE,	spareW1B15(1:15..15):		BOOLEAN¬ FALSE,	-- PROC name, explanation and IO command PROC	commandName(2):			LONG STRING¬ NIL,	explanation(4):			LONG STRING¬ NIL,	cmdProc(6):			PROC¬ NIL];  END... --DiagFlTapeCmdDes		